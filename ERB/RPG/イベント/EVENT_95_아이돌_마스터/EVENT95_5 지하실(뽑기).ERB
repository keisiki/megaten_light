								EXP:(LOCAL):31 = RAND:30
								EXP:(LOCAL):32 = RAND:20
								EXP:(LOCAL):50 = RAND:3
								EXP:(LOCAL):52 = RAND:20
								EXP:(LOCAL):53 = RAND:20
								EXP:(LOCAL):55 = RAND:10
							ENDSELECT
							;2할은 윤간 처녀상실
							SELECTCASE RAND:10
								CASE IS < 2
									CALL ADDS_ABNORMAL_EXP, "처녀상실（윤간）", (LOCAL)
							ENDSELECT
						;남자일 시		
						CASE IS > 2
