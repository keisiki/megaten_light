					SELECTCASE RAND:10
						CASE IS < 3
							TALENT:(LOCAL):남자 = 0
							;가슴 사이즈 설정
							SELECTCASE RAND:100
								CASE IS < 6
									TALENT:(LOCAL):절벽 = 1
								CASE IS < 26
									TALENT:(LOCAL):빈유 = 1
								CASE IS < 46
									TALENT:(LOCAL):거유 = 1
								CASE IS < 52
									TALENT:(LOCAL):폭유 = 1
							ENDSELECT
							;1할은 처녀
							SELECTCASE RAND:10
							CASE IS < 2
								TALENT:(LOCAL):처녀 = 1
							CASE IS > 1
								EXP:(LOCAL):0 = RAND:100
								EXP:(LOCAL):1 = RAND:100
								EXP:(LOCAL):2 = RAND:50
								EXP:(LOCAL):4 = RAND:100
								EXP:(LOCAL):5 = RAND:30
								EXP:(LOCAL):6 = RAND:100
								EXP:(LOCAL):11 = RAND:100
