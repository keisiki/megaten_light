		;	CFLAG:ARG:입력行動 = 2956
		;	CALL RANDOM_TARGET, ARG, CFLAG:ARG:입력行動
		;	RETURN 1
		SELECTCASE RAND:100
			;40％　투섬타임
			CASE IS < 40
				CALL CHECK_ACTIONABLE,ARG,2951
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력行動 = 2951
				CALL RANDOM_TARGET,ARG,2951
				RETURN 1
			;30％　라운드트립
			CASE IS < 70
				CALL CHECK_ACTIONABLE,ARG,2953
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력行動 = 2953
				CALL RANDOM_TARGET,ARG,2953
				RETURN 1
				
			;20％　비구가드브레이크
			CASE IS < 90
				CALL CHECK_ACTIONABLE,ARG,129
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력行動 = 129
				CALL RANDOM_TARGET,ARG,129
				RETURN 1
			;10％　홀리스타
			CASE IS < 100
				CALL CHECK_ACTIONABLE,ARG,2952
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력行動 = 2952
				CALL RANDOM_TARGET,ARG,2952
				RETURN 1
		ENDSELECT
	ELSEIF CFLAG:ARG:行動した횟수 == 1
		SELECTCASE RAND:100
			;70％투섬타임
			CASE IS < 70
				CALL CHECK_ACTIONABLE,ARG,2951
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력行動 = 2951
				CALL RANDOM_TARGET,ARG,2951
				RETURN 1
			;30％휠윈드
			CASE IS < 100
				CALL CHECK_ACTIONABLE,ARG,2954
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력行動 = 2954
				CALL RANDOM_TARGET,ARG,2954
				RETURN 1
		ENDSELECT
	ENDIF
ELSE
	$DECIDE_ACTION
	CALL SEARCH_ACT,ARG
	IF RESULT == 0
		CFLAG:ARG:입력行動 = -1
		CFLAG:ARG:방어フラグ = 1
		RETURN 0
	ENDIF
;	PRINTFORML [{CFLAG:ARG:5}]の스킬数 {RESULT}
	LOCAL = RAND:RESULT
	;스킬번호決定
	LOCAL:1 = S:(10+LOCAL)
;	PRINTFORML [{CFLAG:ARG:5}]の스킬 {LOCAL:1}
	$TARGET_LOOP
	
	CALL RANDOM_TARGET,ARG,LOCAL:1
	CALLFORM SKILL_TARGET_{LOCAL:1}
	;ここで스킬번호を行動内容に代入
	SIF CFLAG:ARG:ターゲット == -1
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력行動 = LOCAL:1

ENDIF
