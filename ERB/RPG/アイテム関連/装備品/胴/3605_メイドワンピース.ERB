@전투능력수정_3605,ARG,ARG:1 = -1

SELECTCASE ARG
	CASE 2
		SIF ARG:1 == -1
			RETURN 13
		IF EQUIP:(ARG:1):머리 == 3205 && EQUIP:(ARG:1):발 == 4603
			LOCAL = BASE:(ARG:1):LV * 3 / 2 + 13
			SIF TALENT:(ARG:1):헌신적
				LOCAL = LOCAL * 5 / 4
			RETURN LOCAL
		ELSE
			RETURN 9
		ENDIF
	CASE 3
		SIF ARG:1 == -1
			RETURN 6
		IF EQUIP:(ARG:1):머리 == 3205 && EQUIP:(ARG:1):발 == 4603
			LOCAL = BASE:(ARG:1):LV / 2
			SIF TALENT:(ARG:1):헌신적
				LOCAL = LOCAL * 5 / 4
			RETURN LOCAL
		ELSE
			RETURN 6
		ENDIF
ENDSELECT
RETURN 0

