;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@kasou_ENTER,ARG,ARG:1
;DA:ARG:(ARG:1)
;ARG、ARG:1はそれぞれ대상のマスのX、Y座標を表しています。
;この던전ではエレベーターの扉が一度右側から開けておかないと左側からは開けられない、という処理に사용しています。
SELECTCASE FLAG:現M
	CASE 35
		IF ARG == 16 && ARG:1 == 9 && 던전フラグ:13:15 < 7
			CALL MESSAGE_WINDOW_D, "", "＞０５のマークの扉は閉ざされている"
			RETURN 0
		ENDIF
	CASE 49
		IF ARG:1 == 25 && !(GETBIT(던전フラグ:13:20,8) && GETBIT(던전フラグ:13:20,9))
			CALL MESSAGE_WINDOW_D, "", "＞扉は閉ざされている"
			RETURN 0
		ENDIF
	CASE 53
		IF ARG:1 == 1 && 던전フラグ:13:19 == 9
			CALL MESSAGE_WINDOW_D, "", "＞早く탈출しないといけない"
			RETURN 0
		ENDIF
		
ENDSELECT
RETURN 1
;=======================================================================
;仮想空間タイル
;=======================================================================
;이벤트タイル特殊표시4
@kasou_TILE, ARG
SELECTCASE ARG / 10
	CASE 4600
		SETCOLOR 0x00ff00
		RESULTS = 双
	CASE 4601
		SETCOLOR 0x00ff00
		RESULTS = ゲ
	CASE 4602
		SETCOLOR 0x00ff00
		RESULTS = ロ
	CASEELSE
		IF (ARG / 10) % 100 == 0
			SETCOLOR 0x00ff00
			RESULTS = 戻
		
		ELSE
			RESULTS = ！
		ENDIF
ENDSELECT

;이벤트タイル特殊표시9
@kasou_TILE2, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASEELSE
		RESULTS = □
ENDSELECT

