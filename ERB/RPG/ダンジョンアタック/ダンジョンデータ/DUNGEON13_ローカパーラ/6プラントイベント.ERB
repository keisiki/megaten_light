;이벤트タイル特殊표시4
@plant_TILE, ARG
SELECTCASE ARG / 10
	CASE 4000
		SETCOLOR 0x00ff00
		RESULTS = ｍ
	CASE 4001
		SETCOLOR 0x00ff00
		RESULTS = ｍ
	CASE 4002
		SETCOLOR 0x00ff00
		RESULTS = ｱﾃ
	CASE 4204
		IF GETBIT(던전フラグ:13:18,3)
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = Ｈ
		ENDIF
	CASE 4202
		SETCOLOR 0x00ff00
		RESULTS = サ
	CASE 4207
		SETCOLOR 0x00ff00
		RESULTS = ゲ
		
		
	CASE 501
		SETCOLOR 0x00ff00
		RESULTS = ア
	CASE 4300
		SETCOLOR 0x00ff00
		RESULTS = 双
	CASE 4301
		SETCOLOR 0x00ff00
		RESULTS = 真
	CASE 4303
		SETCOLOR 0x00ff00
		RESULTS = ロ
	CASE 4304
		IF GETBIT(던전フラグ:13:18,3)
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = Ｈ
		ENDIF
	CASE 4305
		SETCOLOR 0x00ff00
		RESULTS = サ
	CASEELSE
		RESULTS = ！
ENDSELECT

