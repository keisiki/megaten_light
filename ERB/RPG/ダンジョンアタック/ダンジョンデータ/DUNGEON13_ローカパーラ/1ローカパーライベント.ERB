;이벤트タイル特殊표시
@Lokapala_TILE, ARG
SELECTCASE ARG / 10
	CASE 0
		IF 던전フラグ:13:1 > 11
			RESULTS = □
		ELSEIF 던전フラグ:13:1 > 8
			SETCOLOR 0x00ff00
			RESULTS = 双
		ELSEIF 던전フラグ:13:1 > 2
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = ロ
		ENDIF
	;ア디무ル
	CASE 2
		IF 던전フラグ:13:1 > 16
			SETCOLOR 0x00ff00
			RESULTS = ｱﾃ
		ELSEIF 던전フラグ:13:1 > 8
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = ｱﾃ
		ENDIF
	CASE 1, 3
		SETCOLOR 0x00ff00
		RESULTS = ｍ
	CASE 4
		IF 던전フラグ:13:1 > 0
			SETCOLOR 0x00ff00
			RESULTS = ｍ
		ELSE
