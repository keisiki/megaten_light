;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@tamago_ENTER,ARG,ARG:1
;DA:ARG:(ARG:1)
;ARG、ARG:1はそれぞれ대상のマスのX、Y座標を表しています。
;この던전ではエレベーターの扉が一度右側から開けておかないと左側からは開けられない、という処理に사용しています。
SELECTCASE FLAG:現M
	CASE 3
		IF 던전フラグ:13:25 && 던전フラグ:13:2 < 6
			IF ARG:1 < 13 || ARG:1 > 25
				CALL MESSAGE_WINDOW_D, "", "＞彼女を探さなくては…"
				RETURN 0
			ENDIF
			
		ENDIF
ENDSELECT
RETURN 1
;=======================================================================
;たまごタイル
;=======================================================================
;이벤트タイル特殊표시4
@tamago_TILE, ARG,ARG:1,ARG:2
SELECTCASE ARG / 10
	;게일
	CASE 5400
		SETCOLOR 0x00ff00
		RESULTS = ゲ
	;서프
	CASE 5401
		SETCOLOR 0x00ff00
		RESULTS = サ
	;류지
	CASE 5402
		SETCOLOR 0x00ff00
		RESULTS = 竜
	;新島　마코토
	CASE 5403
		SETCOLOR 0x00ff00
		RESULTS = 真
	CASEELSE
		RESULTS = ！
ENDSELECT

;이벤트タイル特殊표시9
@tamago_TILE2, ARG,ARG:1,ARG:2
SELECTCASE ARG / 10
	CASE 5400
		RESULTS = ↑
	CASE 5500
		RESULTS = ↓
	CASE 410
		RESULTS = ←
	CASE 500
		RESULTS = →
	CASEELSE
		RESULTS = □
ENDSELECT

