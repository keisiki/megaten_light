;=======================================================================
;[5]조사　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_4
;==========
;出입の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;==========
;이벤트の処理
;==========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 4
	CALL EVENT_4
	RETURN 1
ELSE
	CALL MESSAGE_WINDOW_D, "SYSTTEM", "아무 것도 없는 것 같다"
	RETURN 0
ENDIF

;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_4,ARG
#LOCALSIZE 10
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出입の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;이벤트の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 4
	CALL EVENT_4
	RETURN 1
;========
;ワープの処理
;========
