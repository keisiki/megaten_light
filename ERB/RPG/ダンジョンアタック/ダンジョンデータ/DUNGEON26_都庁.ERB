;=======================================================================
;[5]조사　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_26

;==========
;出입の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 3
	CALL TREASURE_26
	RETURN RESULT
	
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 6
	CALL UPSTAIRS_26
	RETURN RESULT

ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 7
	CALL DOWNSTAIRS_26
	RETURN RESULT

ELSE
	CALL MESSAGE_WINDOW_D, "", "＞아무 일도 일어나지 않았다"
	RETURN 0
ENDIF

;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_26,ARG
#DIM LCOUNT
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出입の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 3
	CALL TREASURE_26
	RETURN RESULT

;========
;罠の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 5
	CALL TRAP_26
	RETURN RESULT
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 6
	CALL UPSTAIRS_26
	RETURN RESULT

ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 7
	CALL DOWNSTAIRS_26
	RETURN RESULT


;==========
;扉の処理
;==========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 2
	CALL DUNGEON_DOOR,ARG
	RETURN 1


;========================
;이벤트
;========================
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 4
	SELECTCASE FLAG:現M
		CASE 4
			CALL SHOW_PICTURE, "D"
			IF 던전フラグ:(FLAG:現던전):0 > 2 || (던전フラグ:(FLAG:現던전):0 == 2 && 던전フラグ:(FLAG:現던전):1 == 2)
				CALL MESSAGE_WINDOW_D, "", "＞아무도 없다……"
				SIF 던전フラグ:(FLAG:現던전):0 == 2 || 던전フラグ:(FLAG:現던전):0 == 4
					CALL MESSAGE_WINDOW_D, "", "＞스사노오는 옥상으로 향한 것 같다"
				RETURN 1
			ELSEIF 던전フラグ:(FLAG:現던전):0 == 0
				CALL MESSAGE_WINDOW_D, "", "＞체격이 좋은, 거칠어 보이는 남자가 서 있다"
				IF 던전フラグ:(FLAG:現던전):1 == 0
					CALL MESSAGE_WINDOW_D, "파괴신 스사노오", "오우, 형님의 부하인가！/……뭐야, 아닌 것인가/나는 스사노오, 천신 최강의 남자다!"
					CALL MESSAGE_WINDOW_D, "파괴신 스사노오", "누님과 아무래도 한 번 자보고 싶어서、/형님과 협력해 꼭대기에 일단/봉인한 것은 좋았지만/그 놈, 최초로 누님과 하는 것은 자신이라고 말하고 있어!"
