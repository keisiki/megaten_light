;=======================================================================
;스타ト時이벤트
;=======================================================================
@EVENT_DUNGEON_START_26
CALL SHOW_PICTURE, "D"
CALL MESSAGE_WINDOW_D, "", "＞이계화된 도청에 발을 내디뎠다"
IF 던전フラグ:(FLAG:現던전):0 == 4 && 던전フラグ:(FLAG:現던전):2 == 3
	CALL MESSAGE_WINDOW_D, "", "＞스사노오와 아마테라스는 어떡하고 있을까"
ELSEIF 던전フラグ:(FLAG:現던전):1 == 4 && 던전フラグ:(FLAG:現던전):2 == 3
	CALL MESSAGE_WINDOW_D, "", "＞츠쿠요미와 아마테라스는 어떡하고 있을까"
ENDIF
;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_26,ARG,ARG:1
IF FLAG:現M == 0 && ARG == 9 && ARG:1 == 2
	IF 던전フラグ:(FLAG:現던전):2 >= 1
		RETURN 1
	ENDIF
	IF (던전フラグ:(FLAG:現던전):0 >= 2 && 던전フラグ:(FLAG:現던전):1 >= 2) || 던전フラグ:(FLAG:現던전):0 == 4 || 던전フラグ:(FLAG:現던전):1 == 4
		IF 던전フラグ:(FLAG:現던전):2 == 0
			CALL MESSAGE_WINDOW_D, "", "＞옥상으로 통하는 엘리베이터의 결계가 풀렸다"
			던전フラグ:(FLAG:現던전):2 = 1
		ENDIF
		RETURN 1
	ELSE
		CALL MESSAGE_WINDOW_D, "", "＞결계가 쳐져있어 앞으로 나아갈 수 없다"
		RETURN 0
	ENDIF
	
ELSE
	CALL CAN_ENTER,ARG,ARG:1
	RETURN RESULT
ENDIF
