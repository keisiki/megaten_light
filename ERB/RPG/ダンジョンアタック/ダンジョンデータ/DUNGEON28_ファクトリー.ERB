;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_28,ARG,ARG:1
RESULT = 1
SELECTCASE DA:ARG:(ARG:1)
	CASE 12
		;監視塔入り입
		IF BASE:MASTER:지혜 >= 10
			RETURN 1
		ELSE
			RETURN 0
		ENDIF
	CASE 11002
		IF 던전フラグ:[[던전:팩토리]]:7 == 2
			RETURN 1
		ELSEIF 던전フラグ:[[던전:팩토리]]:7 == 1
			CALL MESSAGE_WINDOW_D , "" , "＞地下通路の鍵を使って扉を開けた"
			던전フラグ:[[던전:팩토리]]:7 = 2
			RETURN 1
		ELSE
			CALL MESSAGE_WINDOW_D , "" , "＞扉は閉ざされている"
			RETURN 0
		ENDIF
	CASE 11012, 11022, 11032
		IF !GETBIT(던전フラグ:[[던전:팩토리]]:14 , DA:ARG:(ARG:1) / 10 % 10 -1)
			CALL MESSAGE_WINDOW_D , "" , "＞電子ロックされた扉がある"
			CALL MESSAGE_WINDOW_D , "声" , "＞パスコードを입력してください" , "/" , "ログを残す/ボタンを利用する/再利用しない"
			CALL INPUT_MANY , 0 , 9999
			IF (DA:ARG:(ARG:1) == 11012 && RESULT == 9192) || (DA:ARG:(ARG:1) == 11022 && RESULT == 1213) || (DA:ARG:(ARG:1) == 11032 && RESULT == 1834)
				CALL MESSAGE_WINDOW_D , "" , "＞………/＞……/＞…"
				CALL MESSAGE_WINDOW_D , "声" , "＞パスコードを確認/　ロックを해제します"
				SETBIT 던전フラグ:[[던전:팩토리]]:14 , DA:ARG:(ARG:1) / 10 % 10 -1
				RETURN 1
			ELSE
				CALL MESSAGE_WINDOW_D , "" , "＞………/＞……/＞…"
				CALL MESSAGE_WINDOW_D , "声" , "＞パスコードが違います"
				RETURN 0
			ENDIF
		ELSE
			RETURN 1
		ENDIF
		
	CASEELSE
		CALL CAN_ENTER,ARG,ARG:1
ENDSELECT
RETURN RESULT

