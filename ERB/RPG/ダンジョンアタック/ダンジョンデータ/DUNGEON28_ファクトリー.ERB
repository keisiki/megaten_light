			ABL:ARG:스킬7 = [[스킬:하마온]]
	CASEELSE
ENDSELECT


;=======================================================================
;윤간참가악마
;=======================================================================
@DUNGEON_RAPE_DEVIL_28
;랜덤にどの악마が出るか決定
;RETURN CSV番号 , 人数 , 레벨
SELECTCASE RAND:6
	CASE 0
		RETURN [[キャラ:부쳐]] , 3 + RAND:10 , CSVBASE([[キャラ:부쳐]] , GETNUM(BASE , "LV") , 0) + RAND:3
	CASE 1
		RETURN [[キャラ:데미난디]] , 3 + RAND:10 , CSVBASE([[キャラ:데미난디]] , GETNUM(BASE , "LV") , 0) + RAND:3
	CASE 2
		RETURN [[キャラ:템플나이트]] , 3 + RAND:10 , CSVBASE([[キャラ:템플나이트]] , GETNUM(BASE , "LV") , 0) + RAND:3
	CASE 3
		RETURN [[キャラ:광신자]] , 3 + RAND:10 , CSVBASE([[キャラ:광신자]] , GETNUM(BASE , "LV") , 0) + RAND:3
	CASE 4
		RETURN [[キャラ:아크엔젤]] , 3 + RAND:10 , CSVBASE([[キャラ:아크엔젤]] , GETNUM(BASE , "LV") , 0) + RAND:3
	CASE 5
		RETURN [[キャラ:슬레이브]] , 3 + RAND:10 , CSVBASE([[キャラ:슬레이브]] , GETNUM(BASE , "LV") , 0) + RAND:3
ENDSELECT










