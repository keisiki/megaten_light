;=======================================================================
;スタート時이벤트
;=======================================================================
@EVENT_DUNGEON_START_28
;フィールドマップを全アナライズ
IF 던전フラグ:[[던전:팩토리]]:8 == 0
	FLAG:現M = 0
	CALL MAKE_FLOOR, 28
	FOR LOCAL , 0 , 31
		FOR LOCAL:1 , 0 , 41
			CALL SET_FLOOR_ANALYZE, 2 , [[던전:팩토리]] , 0 , LOCAL:1, LOCAL
		NEXT
	NEXT
	FLAG:現M = 1
	CALL MAKE_FLOOR, 28
	던전フラグ:[[던전:팩토리]]:8 = 1
ENDIF
;키쿠에の상태
IF 던전フラグ:[[던전:팩토리]]:5 > -1 && 의뢰フラグ:13:0 == 2
	SELECTCASE DAY - STRFLAG_NUM_REQ("의뢰を受けた日" , , , , 13)
		;二日以内なら完全に無事
		CASE IS <= 2
			던전フラグ:[[던전:팩토리]]:5 = 1
		;５日以内なら助かる
		CASE IS <= 5
			SIF 던전フラグ:[[던전:팩토리]]:5 < 2
				던전フラグ:[[던전:팩토리]]:5 = 2
		;１０日までは重度の洗脳
		CASE IS <= 10
			던전フラグ:[[던전:팩토리]]:5 = 3
		;それ以後は붕괴
		CASEELSE
			던전フラグ:[[던전:팩토리]]:5 = 4
		
	ENDSELECT
ENDIF

