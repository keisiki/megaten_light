	ELSE
		CALL MAGIC_BOX , LOCAL , GETNUM(ITEM, LOCALS:1) , GETNUM(ITEM, LOCALS:2) , LOCAL:1 , FLAG:現던전, FLAG:現M + 70, (DA:(FLAG:現X):(FLAG:現Y) / 10) % 100
	ENDIF
