@SET_ENEMY_STATUS_28, ARG
#LOCALSIZE 1
SELECTCASE NO:ARG
	CASE [[キャラ:엔젤]]
		ABL:ARG:스킬1 = [[스킬:디아라마]]
		ABL:ARG:스킬2 = [[스킬:마한마]]
		ABL:ARG:스킬3 = [[스킬:갈라]]
		ABL:ARG:스킬6 = [[스킬:마하・갈]]
