@ENCOUNT_RATE_28
SELECTCASE FLAG:現M
	;팩토리
	CASE 0
		FLAG:エンカウント率 = MAX(FLAG:未遭遇歩数 * 3 - 15, 0)
		FLAG:MAG소비율 = 0
		FOR LOCAL , 1 , 7
			SIF POS(LOCAL) > -1
				PALAM:POS(LOCAL):욕정 += (ABL:POS(LOCAL):욕망+ 1) * 5
		NEXT
	;タウン、教会、労働者寮、強制収容所
	CASE 1, 2, 3
		FLAG:未遭遇歩数 = 0
		FLAG:エンカウント率 = 0
		FLAG:MAG소비율 = 0
	CASEELSE
		FLAG:エンカウント率 = MAX(FLAG:未遭遇歩数 * 5 - 25, 0)
