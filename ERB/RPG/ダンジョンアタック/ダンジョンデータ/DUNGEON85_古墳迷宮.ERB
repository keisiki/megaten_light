;=======================================================================
@PREREQUISITE_DUNGEON_85
;市庁をクリアして、
IF 던전フラグ:81:0 == 1 && !던전フラグ:85:0
	;쿠미코を助けていて함락
	LOCAL = GETCHARA([[キャラ:쿠미코]])
		IF 던전フラグ:78:1 == 1 && FINDCHARA_B([[キャラ:쿠미코]]) == 1 && 함락(LOCAL)
		RETURN 1
	;쿠미코を助けていない
	ELSEIF 던전フラグ:78:1 == 2
		RETURN 1
	ENDIF
ENDIF

