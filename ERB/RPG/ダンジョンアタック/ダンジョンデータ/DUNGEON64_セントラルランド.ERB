@EVENT_DUNGEON_64, ARG, ARG:1
SELECTCASE DA:(FLAG:現X):(FLAG:現Y) % 10
;==========
;ワープの処理
;==========
	CASE 1
	CALL FLOOR_64
	RETURN RESULT
	
	CASE 9
		SELECTCASE DA:(FLAG:現X):(FLAG:現Y) / 10
