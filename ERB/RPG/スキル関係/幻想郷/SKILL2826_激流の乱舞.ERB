;통상공격
;스킬のテン플레이트になっています

;스킬名
@SKILL_NAME_2826,ARG
RESULTS = 격류의난무

;　명중
@SKILL_HITRATE_2826, ARG
RETURN 90

;　위력
;仕様上,1명相손なら1~3回になるのでダイン相当の320
;上限HIT数に引っかからなければ1명あたりの期待値は240
@SKILL_POWER_2826, ARG
RETURN 160

;최저がここで渋いのは同랭크にあたるマハ系が최대5HITしかできないから
;적総数の판정を全후열합계にできるという強みもあるので、ここで少し絞っといても総HIT数は割合稼げる…はず
;최저공격횟수
@SKILL_MINATTACKNUMBER_2826,ARG
RETURN 2

;　최대공격횟수
@SKILL_MAXATTACKNUMBER_2826,ARG
RETURN 8

;一体辺りの최대ＨＩＴ数
@SKILL_MAXATK_PER_2826,ARG
RETURN 3

;一体辺りの최저ＨＩＴ数
@SKILL_MINATK_PER_2826,ARG
RETURN 0

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_2826, ARG
RETURN 3
;소비なし＝1

;　コストの量
@SKILL_COST_2826,ARG
RETURN 15

;　相性
@SKILL_TYPE_2826,ARG
RETURN GET_TYPE_NUM("수격")
;수격

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_2826, ARG
RETURN 1
;EXTRA

;　1물리데미지で計算するか2마법데미지で計算するか
@SKILL_DAMAGETYPE_2826, ARG
RETURN 2


;　대상が1적か2아군か3場전체か4特殊か
@SKILL_TARGET_2826
RETURN 1

;　사거리
@SKILL_RANGE_2826,ARG
RETURN 3

;　범위　11명　2１열　3전체　4　적아군전체
@SKILL_SPHERE_2826,ARG
RETURN 3

;　 効果の타입　1공격、2회복、3상태변화、4特殊、5상태회복
@SKILL_EFECT_2826
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_2826,ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;계승타입
@SKILL_SUCCESSION_TYPE_2826
RETURN GET_SUCCESSION_NUM("수격")
