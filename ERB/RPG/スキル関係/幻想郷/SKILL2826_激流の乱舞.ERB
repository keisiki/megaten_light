;속도보정
@SKILL_SPEED_2826, ARG = -1
RETURN 0

;　구상の표시とその타입
@SKILL_MESSAGE_2826,ARG

;전투時以外に사용できるか
@SKILL_ACTIONABLE_FIELD_2826
RETURN 0

;스킬변화？用랭크
@SKILL_RANK_2826
RETURN 7

@SKILL_RANKUP_2826
;랭크アップなし
RETURN 0


