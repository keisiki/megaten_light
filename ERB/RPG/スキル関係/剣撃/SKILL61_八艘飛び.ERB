;팔척뛰기, 페르소나 요시츠네 전용

;스킬名
@SKILL_NAME_61, ARG
RESULTS = 팔척뛰기

;　명중
@SKILL_HITRATE_61, ARG
RETURN 90

;　위력
@SKILL_POWER_61, ARG
; ボス사용時のみ위력150
SIF ARG >= 0 && CFLAG:ARG:ボスフラグ
	RETURN 150
RETURN 100

;　최저공격횟수
@SKILL_MINATTACKNUMBER_61, ARG
RETURN 8

;　최대공격횟수
@SKILL_MAXATTACKNUMBER_61, ARG
RETURN 8

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_61, ARG
RETURN 3
;소비なし＝1

;　コストの量
@SKILL_COST_61, ARG
RETURN 50

;　相性
@SKILL_TYPE_61, ARG
RETURN GET_TYPE_NUM("검격")

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_61, ARG
RETURN 1

;　1물리데미지で計算するか2마법데미지で計算するか
@SKILL_DAMAGETYPE_61, ARG
RETURN 1

;　대상が1적か2아군か3場전체か4特殊か
@SKILL_TARGET_61
RETURN 1

;　사거리
@SKILL_RANGE_61, ARG
RETURN 3

;　범위　11명　2１열　3전체　4　적아군전체
@SKILL_SPHERE_61, ARG
RETURN 3

;　 効果の타입　1공격、2회복、3상태변화、4特殊、5상태회복
@SKILL_EFECT_61
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_61, ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;계승타입
@SKILL_SUCCESSION_TYPE_61
RETURN 0
;검격

@継承不能_61
RETURN 1

;속도보정
@SKILL_SPEED_61, ARG = -1
RETURN 0

;　구상の표시とその타입
@SKILL_MESSAGE_61, ARG


;전투時以外に사용できるか
@SKILL_ACTIONABLE_FIELD_61
RETURN 0

;스킬변화？用랭크
@SKILL_RANK_61
RETURN 0

;テン플레이트用、추가효과シリーズ
;추가효과스테이터스
@SKILL_ADDTIONAL_STATE_61, ARG
RETURN 0

;추가효과상성
@SKILL_ADDTIONAL_TYPE_61, ARG
RETURN 0

;추가효과기본부여확률
@SKILL_ADDTIONAL_BASE_RATE_61, ARG
RETURN 0

;추가효과부여확률上限
@SKILL_ADDTIONAL_MAX_RATE_61, ARG
RETURN 0

;====================================================
;스킬及び行動の実行
;====================================================
@ACTION_61, ARG, ARG:1
IF CFLAG:ARG:ボスフラグ
	CALL ATTACK_FIELD, ARG, ARG:1, 61
ELSE
	CALL ATTACK_RAND, ARG, ARG:1, 61
ENDIF

