@SKILL_ADDTIONAL_STATE_3618,ARG
RETURN GET_STATE_NUM("SHOCK")

;추가효과상성
@SKILL_ADDTIONAL_TYPE_3618,ARG
RETURN GET_TYPE_NUM("전격")

;추가효과기본부여확률
