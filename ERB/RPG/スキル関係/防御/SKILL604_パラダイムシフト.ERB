;====================================================
;스킬及び行動の実行
;====================================================
@ACTION_604,ARG,ARG:1

;実行


IF FLAG:ARG > -1
	FOR LOCAL,40,57
		MAXBASE:ARG:LOCAL = (RAND:6-2)*100
		SIF MAXBASE:ARG:LOCAL == -200
			MAXBASE:ARG:LOCAL = 999
		SIF MAXBASE:ARG:LOCAL == 200
			MAXBASE:ARG:LOCAL = 150
		SIF MAXBASE:ARG:LOCAL == 300
			MAXBASE:ARG:LOCAL = 200
	NEXT
	PRINTFORMW %CALLNAME:ARG%의 상성이 변했다!
ENDIF






