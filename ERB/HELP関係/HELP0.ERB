@DICTIONARY_MENU
DRAWLINE
PRINTL 어느 항목에 대해서 조사합니까？
PRINTLC [ 7] 그 이외의 수치
PRINTLC [ 9] 조교 아이템
PRINTLC [10] 체질 변화약
PRINTLC [11] SHOP 커맨드 설명
PRINTLC [12] OPTION 설명
PRINTLC [13] 미션 로그
PRINTLC [14] 함락 이력
PRINTLC [15] 장부열람
PRINTLC [98] 미리니름Ｑ＆Ａ(작성중)
PRINTLC [99] 초심자에게 권고
PRINTL 
PRINTL [100]그먄둔다
DRAWLINE
$INPUT_LOOP
INPUT
IF RESULT == 100
	RETURN 0
ELSEIF RESULT == 7
	;그외数値
	CALL DICTIONARY_OTHER_MAIN
ELSEIF RESULT == 9
	;아이템
	CALL DICTIONARY_ITEM_MAIN
ELSEIF RESULT == 10
	;体質변화薬
	CALL DICTIONARY_DRUG_MAIN
ELSEIF RESULT == 11
	;SHOPコマンド설명
	CALL DICTIONARY_SHOP_COMMAND
ELSEIF RESULT == 12
	;OPTION설명
	CALL DICTIONARY_CONFIG_MAIN
ELSEIF RESULT == 13
	;ミッションログ
	CALL DICTIONARY_ENDING_MAIN
ELSEIF RESULT == 14
	;함락내역
	CALL DICTIONARY_FALLED_MAIN
ELSEIF RESULT == 15
	;帳簿閲覧
	CALL ACCOUNTING_BOOKS_MAIN
ELSEIF RESULT == 98
	;ネタばれＱ＆Ａ
	IF FLAG:周回횟수 >= 1 || FLAG:5 == 9
		CALL DICTIONARY_NETABARE_QA
	ELSE
	PRINTW 아직 열람할 수 없습니다.
	GOTO INPUT_LOOP
	ENDIF
ELSEIF RESULT == 99
	;初心者Ｑ＆Ａ
	CALL DICTIONARY_EASY_QA
ELSE
	GOTO INPUT_LOOP
ENDIF
RESTART



;=============================================================================
;用語辞典――그외数値
;=============================================================================
;--------------------------------------------------
;그외数値해설メイン페이지
;--------------------------------------------------
@DICTIONARY_OTHER_MAIN
P = 0
$PRINT_LIST
DRAWLINE
PRINTFORML 능력이나 각인이나 경험이나 주 이외의 수치에 관한 대략의 설명입니다.
PRINTFORML 내용을 알고 싶은 항목의 번호를 입력해 주세요. ＜page.{P+1}/2＞
DRAWLINE
IF P == 0
	PRINTL [ 0] 체력
	PRINTL [ 1] 기력
	PRINTL [ 2] 사정게이지
	PRINTL [ 3] 모유게이지
	PRINTL [ 4] 절정게이지
	PRINTL [ 5] 충성도
	PRINTL [ 6] 스트레스
	PRINTL [ 7] 자금
	PRINTL [ 8] 마화
ELSEIF P == 1
	PRINTL [10] LV
	PRINTL [11] 힘
	PRINTL [12] 지혜
	PRINTL [13] 마력
	PRINTL [14] 인내력
	PRINTL [15] 속도
	PRINTL [16] 운
	PRINTL [17] 각 내성
	PRINTL [18] ＭＡＧ
	PRINTL [19] CP
ENDIF
DRAWLINE
PRINTFORMLC \@(P <= 0) ? %" " * 18% # [1001]이전 페이지\@
PRINTLC [1000]돌아간다
PRINTFORMLC \@(P >= 1) ? %" " * 18% # [1009]다음 페이지\@
$INPUT_LOOP
INPUT
IF RESULT == 1000
	RETURN 0
ELSEIF RESULT == 1001 && P > 0
	P -= 1
ELSEIF RESULT == 1009 && P < 1
	P += 1
ELSEIF RESULT < 0 || RESULT > 19
	GOTO INPUT_LOOP
ELSE
	Q = RESULT
	TRYCALLFORM DICTIONARY_OTHER_{Q}
ENDIF
GOTO PRINT_LIST

