;ARGは生成キャラの등록번호
@RANDOMCHARA_4905, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "一人称"
		RESULTS = \@IS_MALE(ARG) ? オレサマ#アタイ\@
	CASE "二人称"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		RETURN RAND(1200, 2400)
	CASE "기력"
		RETURN RAND(800, 1500)
	CASE "속성"
		RETURN RAND(50, 120), RAND(0, 85)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
;데빌시프터、モヒカン系、닌자系、破戒僧系で大別 비전투원は存在しない
		SELECTCASE RAND:10
			CASE IS < 3
				RESULTS = 데빌시프터
				RETURN 1
			CASE IS < 5
				RESULTS = 석세서
				RESULTS:1 = 건슬링거
				RESULTS:2 = 도구지식
				RETURN 1, 1, 3
			CASE IS < 7
				RESULTS = 이능자
				RESULTS:1 = 도구지식
				RETURN 1, 1
			CASEELSE
				RESULTS = 서머너
				RESULTS:1 = 이능자
				RETURN 1, 1
		ENDSELECT
	CASE "링크악마"
		IF TALENT:ARG:데빌시프터 == 1
			IF TALENT:ARG:남자 == 1
				SELECTCASE RAND:5
					CASE 0
						RETURN [[キャラ:슬라임]]
					CASE 1
						RETURN [[キャラ:잭프로스트]]
					CASE 2
						RETURN [[キャラ:오니]]
					CASE 3
						RETURN [[キャラ:인큐버스]]
					CASE 4
						RETURN [[キャラ:이누가미]]
				ENDSELECT
			ELSE
				SELECTCASE RAND:5
					CASE 0
						RETURN [[キャラ:보디코니언]]
					CASE 1
						RETURN [[キャラ:픽시]]
					CASE 2
						RETURN [[キャラ:하피]]
					CASE 3
						RETURN [[キャラ:서큐버스]]
					CASE 4
						RETURN [[キャラ:네코마타]]
				ENDSELECT
			ENDIF
		ELSE
			RETURN 0
		ENDIF
	CASE "초기스테이터스比率"
		RETURN 10, 10, 8, 7, 7, 5
	CASE "스킬"
		SIF TALENT:ARG:이능자 == 0
			RETURN 0
		SELECTCASE RAND:4
			CASE 0
				RESULT = [[스킬:돌격]]
				RESULT:1 = [[스킬:마구날뛰기]]
				RESULT:2 = [[스킬:돌려차기]]
			CASE 1
				RESULT = [[스킬:번개찌르기]]
				RESULT:1 = [[스킬:베놈재퍼]]
				RESULT:2 = [[스킬:회전베기]]
			CASE 2
				RESULT = [[스킬:지오]]
				RESULT:1 = [[스킬:지온가]]
				RESULT:2 = [[스킬:지오다인]]
			CASE 3
				RESULT = [[스킬:잔]]
				RESULT:1 = [[스킬:잔마]]
				RESULT:2 = [[스킬:잔다인]]
		ENDSELECT
		SELECTCASE RAND:4
			CASE 0
				RESULT:3 = [[스킬:시바브]]
				RESULT:4 = [[스킬:포이즈마]]
				RESULT:5 = [[스킬:마카쟈마]]
				RESULT:6 = [[스킬:마카라칸]]
			CASE 1
				RESULT:3 = [[스킬:하피루마]]
				RESULT:4 = [[스킬:도루미나]]
				RESULT:5 = [[스킬:치유촉진(소)]]
				RESULT:6 = [[스킬:타룬・다]]
			CASE 2
				RESULT:3 = [[스킬:자폭]]
				RESULT:4 = [[스킬:이악물기]]
				RESULT:5 = [[스킬:전격]]
				RESULT:6 = [[스킬:데・카쟈]]
			CASE 3
				RESULT:3 = [[스킬:테트라칸]]
				RESULT:4 = [[스킬:무도]]
				RESULT:5 = [[스킬:마하무도]]
				RESULT:6 = [[스킬:무도온]]
		ENDSELECT
		RESULT:7 = [[스킬:독침]]
		RESULT:8 = [[스킬:마비침]]
		RESULT:9 = [[스킬:몽견침]]
		RETURN RESULT
	CASE "相性"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RETURN GETNUM(TALENT, "광기"), GETNUM(TALENT, "발정가능"), GETNUM(TALENT, "해방"), GETNUM(TALENT, "바늘다루기"), GETNUM(TALENT, "바이"), GETNUM(TALENT, "악취둔감"), GETNUM(TALENT, "쾌감에솔직")
	CASE "除外소질"
		RETURN GETNUM(TALENT, "정조관념"), GETNUM(TALENT, "아픔에약함"), GETNUM(TALENT, "겁쟁이"),GETNUM(TALENT, "헌신적")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
		IF RAND:5 == 0
			RETURN 1
		ELSE
			RETURN 2
		ENDIF
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "体型"
		RETURN 0
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"
		RETURN 0
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "肉体部位"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:3
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:5
			CASE "새드끼"
				RETURN !RAND:4 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:10
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:5
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * RAND:2
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
		ENDSELECT
	CASE "모자"
		IF RAND:5
			RETURN 0
		ELSE
			RETURN [[衣装:모자]]
		ENDIF
	CASE "옷"
		IF IS_MALE(ARG)
			RETURN [[衣装:겉옷]]
		ELSE
			IF RAND:2
				RETURN 0
			ELSE
				RETURN [[衣装:기모노]]
			ENDIF
		ENDIF
	CASE "하의"
		IF IS_MALE(ARG)
			RETURN [[衣装:바지]]
		ELSE
			LOCALS = 옷
			IF CFLAG:ARG:LOCALS == 0
				RETURN 0
			ELSE
				RETURN [[衣装:하카마]]
			ENDIF
		ENDIF
	CASE "전신복"
		LOCALS = 옷
		IF CFLAG:ARG:LOCALS == 0
			RETURN [[衣装:본디지수트]]
		ELSE
			RETURN 0
		ENDIF
	CASE "손"
		RETURN 0
	CASE "속옷（상）"
		IF IS_MALE(ARG)
			RETURN 0
		ELSE
			LOCALS = 옷
			IF CFLAG:ARG:LOCALS == 0
				RETURN [[衣装:브래지어(흑)]]
			ELSE
				RETURN [[衣装:무명천]]
			ENDIF
		ENDIF
	CASE "속옷（하）"
		IF IS_MALE(ARG)
			IF RAND:5
				RETURN [[衣装:트렁크스]]
			ELSE
				RETURN [[衣装:훈도시]]
			ENDIF
		ELSE
			LOCALS = 옷
			IF CFLAG:ARG:LOCALS == 0
				RETURN [[衣装:팬티(흑)]]
			ELSE
				RETURN 0
			ENDIF
		ENDIF
	CASE "전신속옷"
		RETURN 0
	CASE "양말"
		LOCALS = 옷
		IF CFLAG:ARG:LOCALS == 0
			RETURN [[衣装:양말]]
		ELSE
			RETURN [[衣装:버선]]
		ENDIF
	CASE "신발"
		IF IS_MALE(ARG)
			RETURN [[衣装:게다]]
		ELSE
			LOCALS = 옷
			IF CFLAG:ARG:LOCALS == 0
				RETURN [[衣装:부츠]]
			ELSE
				RETURN [[衣装:짚신]]
			ENDIF
		ENDIF
	;악마汎用사용しています0:랜덤 1:女_아이　2:女_소녀　3:女_若者　4:女_大人　5:女_淑女　6:女_古風　7:男_若者　8:男_大人　9:짐승_ケダモノ
	CASE "회화타입"
		SIF TALENT:ARG:광기 == 1 && TALENT:ARG:데빌시프터 == 1
				RETURN 9
		IF TALENT:ARG:남자 == 0
			SELECTCASE RAND:4
				CASE 0
					RETURN 2
				CASE 1
					RETURN 3
				CASE 2
					RETURN 4
				CASE 3
					RETURN 6
			ENDSELECT
		ELSE
			RETURN 0
		ENDIF
	CASE "이름"
		IF (TALENT:ARG:魔乳 == 1 || TALENT:ARG:폭유 == 1)
			RESULTS = 마유(魔乳)의 
		ELSEIF TALENT:ARG:거유 == 1
			RESULTS = 매혹의 
		ELSEIF TALENT:ARG:광기 == 1
			RESULTS = 광기의 
		ELSE
			RESULTS = 쿨한 
		ENDIF
		IF TALENT:ARG:데빌시프터 == 1
			RESULTS += "가이아즈"
		ELSEIF TALENT:ARG:서머너 == 1
			RESULTS += "파계승"
		ELSEIF TALENT:ARG:이능자 == 1
			IF TALENT:ARG:남자 == 1
				RESULTS += "어새신"
			ELSE
				RESULTS += "쿠노이치"
			ENDIF
		ELSE
			RESULTS += "가이아교도"
		ENDIF
	CASE "愛称"
		IF (TALENT:ARG:魔乳 == 1 || TALENT:ARG:폭유 == 1)
			RESULTS = 마유(魔乳)의 
		ELSEIF TALENT:ARG:거유 == 1
			RESULTS = 매혹의 
		ELSEIF TALENT:ARG:광기 == 1
			RESULTS = 광기의 
		ELSE
			RESULTS = 쿨한 
		ENDIF
		IF TALENT:ARG:데빌시프터 == 1
			RESULTS += "가이아즈"
		ELSEIF TALENT:ARG:서머너 == 1
			RESULTS += "파계승"
		ELSEIF TALENT:ARG:이능자 == 1
			IF TALENT:ARG:남자 == 1
				RESULTS += "어새신"
			ELSE
				RESULTS += "쿠노이치"
			ENDIF
		ELSE
			RESULTS += "가이아교도"
		ENDIF
	CASE "해설"
		RESULTS = TOKYO를 중심으로 급속히 세력을 늘리고 있는 신흥 종교 가이아교의 신자
		IF TALENT:ARG:데빌시프터
			RESULTS:1 = 악마의 힘을 믿은 나머지、스스로 악마가 되는 방법을 몸에 익혀 버린 것 같다
		ELSEIF TALENT:ARG:건슬링거
			RESULTS:1 = 힘이 전부라고 믿으며、룰에 얽매이지 않는 무법자다
		ELSEIF TALENT:ARG:서머너
			RESULTS:1 = 악마를 신봉하는 전(前) 성직자이다
		ELSE
			RESULTS:1 = 암살을 생업으로 하는 닌자집단의 일원이며、가이아 교단의 암부를 지탱해왔다
		ENDIF
		RESULTS:2 = 　
		RESULTS:3 = (이하Wikipedia로부터 발췌)
		RESULTS:4 = 가이아 교단은 질서나 계급에 얽매이지 않고、자연스럽게 일체화 하는것을 중요시하고 있다
		RESULTS:5 = 차별도 구별도 없이、관리를 부정해 모든 것과 공존하려고 하고 있다
		RESULTS:6 = 그 때문에 현실적이고 실리적이어서、힘이 없는 자에게 가치를 찾지 않고 구제의 손도 뻗지 않는다
		RESULTS:7 = 기본적으로 실력지상주의의 단체라、자유는 있지만 질서는 존재하지 않는다
		RESULTS:8 = 악마의 존재를 허용하고 공생해 나가는 것을 생각하고 있기 때문에、메시아교와는 정면으로 대립하고 있다
		RESULTS:9 = 혼돈을 존중하므로 야쿠자나 반사회적인 인물도 많아、
		RESULTS:10 = 태도나 언행은 몹시 거칠고 난폭해 폭행을 가하는 일도 주저하지 않는다
		RESULTS:11 = 교도 중에는 암살을 생업으로 하는 사람도 있다。
	;最後に呼び出すので何らかの調整が필요한場合はここで
	CASE "最終調整"
		RETURN 0
ENDSELECT
