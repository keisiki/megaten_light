ELSEIF RESULT == 1
	IF TEQUIP:T:35 == 0
		;음란・창부持ち
		IF TALENT:PLAYER:4 || TALENT:PLAYER:7
			IF PLAYER != T
				IF RAND:2 == 0
					PRINTFORMW %CALLNAME:PLAYER%は淫らな笑みを浮かべて콘돔を取り出すと、%CALLNAME:T%の페니스に입で装着させた…
				ELSE
					PRINTFORMW %CALLNAME:PLAYER%は慣れた손つきで콘돔を咥えると、そのまま입で%CALLNAME:T%の페니스に装着させた…
				ENDIF
			ELSEIF PLAYER == MASTER && ASSI > 0
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW %CALLNAME:PLAYER%は콘돔を取り出すと、%CALLNAME:ASSI%に입で%CALLNAME:T%の페니스に装着させた…
					CASE 1
						PRINTFORMW %CALLNAME:ASSI%は慣れた손つきで콘돔を咥えると、そのまま입で%CALLNAME:T%の페니스に装着した…
					CASE 2
						PRINTFORMW %CALLNAME:PLAYER%は콘돔を取り出すと、%CALLNAME:TARGET%に입で%CALLNAME:T%の페니스に装着させた…
				ENDSELECT
			ELSE
				PRINTFORMW %CALLNAME:PLAYER%は콘돔を取り出すと、%CALLNAME:TARGET%に입で%CALLNAME:T%の페니스に装着させた…
			ENDIF
		ELSE
			PRINTFORMW %조사처리(CALLNAME:T,"는")%、콘돔을 꼈다
		ENDIF
		ITEM:45 -= 1
		TEQUIP:T:35 = 1
	ELSEIF (T == MASTER && (FLAG:98 & 4)) || (T == ASSI && (FLAG:98 & 2)) || (T == TARGET && (FLAG:98 & 1))
		PRINTFORMW %조사처리(CALLNAME:T,"는")%、콘돔을 항상 끼지 않게 했다
		SIF T == MASTER
			FLAG:98 &= 3
		SIF T == ASSI
			FLAG:98 &= 5
		SIF T == TARGET
			FLAG:98 &= 6
	ELSE
		PRINTFORMW %조사처리(CALLNAME:T,"는")%、이미 콘돔을 끼고있다
	ENDIF
ELSEIF RESULT == 2
	PRINTFORMW %조사처리(CALLNAME:T,"는")% 콘돔을 벗었다
	SIF T == MASTER
		FLAG:98 &= 3
	SIF T == ASSI
		FLAG:98 &= 5
	SIF T == TARGET
		FLAG:98 &= 6
	TEQUIP:T:35 = 0
ELSEIF RESULT == 100
	RETURN 0
ELSE
	GOTO INPUT_LOOP1
ENDIF


