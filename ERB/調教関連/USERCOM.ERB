	;名前の長さが0の場合、ないものとみなす
	SIF STRLENS(TRAINNAME:LOCAL) == 0
		CONTINUE
	;派生コマンドは蹴る
	TRYCCALLFORM COMTYPE_{LOCAL}
		SIF RESULT == COMTYPE("派生系")
			CONTINUE
	CATCH
	ENDCATCH
	LOCAL:1++
	;COMABLEを체크
	CALL COM_ABLE_COMMON_D, LOCAL
	SIF RESULT == 0
		CONTINUE
	;派生コマンドになるかどうかを체크
	TRYCCALLFORM JUMP_COM{LOCAL}
		LOCAL:2 = RESULT >= 0 ? RESULT # LOCAL
	CATCH
		LOCAL:2 = LOCAL
	ENDCATCH
	;MENU配列に実行可能なコマンドを登録していく
	Q:(LOCAL:1) = LOCAL:2
	;필터関係は派生前で체크するので注意
	SIF GET_COM_FILTERING(LOCAL, TARGET)
		CONTINUE
	TRYCCALLFORM COMTYPE_{LOCAL}
		SIF CFLAG:(170+RESULT) == 1
			CONTINUE
	CATCH
	ENDCATCH
	;すでに표시されているコマンドがここまで来た場合は帰る
	SIF MATCH(R, LOCAL:2)
		CONTINUE
	;コマンド표시
	TRYCCALLFORM COMNAME{LOCAL:2}
		PRINTFORMC %RESULTS%[{LOCAL:1,3}]
		DEBUGPRINT LOCAL:1,LOCAL:2,LOCAL:3
	CATCH
		PRINTFORMC %TRAINNAME:(LOCAL:2)%[{LOCAL:1,3}]
		DEBUGPRINT LOCAL:1,LOCAL:2,LOCAL:3
	ENDCATCH
	;ここまできたら표시されたものとして登録される
	R:(LOCAL:1) = LOCAL:2
	;改行処理
	CALL CHECK_NEWLINE
NEXT

