
@노역中出し(ARG,ARG:1,ARG:2,ARGS)
SIF CFLAG:(ARG:1):자궁내피임결계
	RETURN
IF ARG == MASTER
	LOCALS = 主人の中出氏による임신판정
ELSE
	;暫定父親の置換処理(かなり適当)
	IF CFLAG:(ARG:1):노역中の中出氏による暫定父親 == -1 || !RAND:3
		CFLAG:(ARG:1):노역中の中出氏による暫定父親 = ARG
	ENDIF
	LOCALS = 顧客の中出氏による임신판정
ENDIF
SELECTCASE ARGS
CASE "="
	CFLAG:(ARG:1):LOCALS = ARG:2
CASE "+"
	CFLAG:(ARG:1):LOCALS += ARG:2
CASE "-"
	CFLAG:(ARG:1):LOCALS -= ARG:2
CASE "*"
	CFLAG:(ARG:1):LOCALS *= ARG:2
CASE "/"
	CFLAG:(ARG:1):LOCALS /= ARG:2
ENDSELECT