
@CLOTHES_NAME_321
RESULTS = 네코마타수트(의상)

;노출部位
@CLOTHES_EXPOSE_321,ARG
;ARG = 노출する部位
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = ヘソ노출 7 = 음순노출 8 = 둔부노출
SELECTCASE ARG
	CASE 5,6,7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비部位
@CLOTHES_PART_321
RETURN 3

;옷の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_321
RETURN 2

;가랑이の구조
;1 = 바지、2 = ビキニ타입、3 = 스커트、4 = 穴開き、5 앞열림、6 開閉가능
@CLOTHES_CROTCH_321
RETURN 2

;장비時の조교効果
@CLOTHES_SOURCE_321,ARG = -1
RETURN 0

;장비時の경험추가
@CLOTHES_EXP_321,ARG = -1
		RETURN 0

@CLOTHES_PRICE_321
RETURN 100
