
@CLOTHES_NAME_312
RESULTS = 본디지수트

;노출部位
@CLOTHES_EXPOSE_312,ARG
;ARG = 노출する部位
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = ヘソ노출 7 = 음순노출 8 = 둔부노출
SELECTCASE ARG
	CASE 0
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비部位
@CLOTHES_PART_312
RETURN 3

;옷の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_312
RETURN 0

;가랑이の구조
;1 = 바지、2 = ビキニ타입、3 = 스커트、4 = 穴開き、5 앞열림、6 開閉가능
@CLOTHES_CROTCH_312
RETURN 4

;장비時の조교効果
@CLOTHES_SOURCE_312,ARG = -1
RETURN 0

;장비時の경험추가
@CLOTHES_EXP_312,ARG = -1
IF FLAG:SHOPコマンド == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <본디지수트ckrdydwnd>\@ ARG == MASTER ? (wndls) #\@\@ ARG == ASSI ? (조수) #\@
	IF TCVAR:ARG:획득가학쾌락경험
		TCVAR:ARG:획득가학쾌락경험 += MAX(TCVAR:ARG:획득가학쾌락경험*140/100 , 1)
		SIF ARG == TARGET
			TIMES DOWNBASE:ARG:1 , 1.20
	ENDIF
	IF TCVAR:ARG:획득고통쾌락경험
		TCVAR:ARG:획득고통쾌락경험 += MAX(TCVAR:ARG:획득고통쾌락경험*140/100 , 1)
		SIF ARG == TARGET
			TIMES DOWNBASE:ARG:1 , 1.20
	ENDIF
ELSE
ENDIF


@CLOTHES_PRICE_312
RETURN 2000
