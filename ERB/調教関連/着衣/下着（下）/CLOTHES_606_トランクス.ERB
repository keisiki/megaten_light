
@CLOTHES_NAME_606
RESULTS = 트렁크스

;노출部位
@CLOTHES_EXPOSE_606,ARG
;ARG = 노출する部位
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = ヘソ노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비部位
@CLOTHES_PART_606
RETURN 6

;옷の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_606
RETURN 0

;가랑이の구조
;1 = 바지、2 = ビキニ타입、3 = 스커트、4 = 穴開き、5 앞열림、6 開閉가능
@CLOTHES_CROTCH_606
RETURN 1

;장비時の조교効果
@CLOTHES_SOURCE_606,ARG = -1
RETURN 0

;장비時の경험추가
@CLOTHES_EXP_606,ARG = -1
		RETURN 0

@CLOTHES_PRICE_606
RETURN 10

;촉각
;1 = スルー 2 = 上から伝わる 4 = 潜り込ませられる
;ARG 0 = Ｃ 1 = Ｖ 2 = Ａ 3 = 유두 4 = 유방
@CLOTHES_촉각_606,ARG
SELECTCASE ARG
	CASE 0 TO 2
		RETURN 6
ENDSELECT
