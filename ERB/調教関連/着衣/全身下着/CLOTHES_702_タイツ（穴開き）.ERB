
@CLOTHES_NAME_702
RESULTS = 타이즈（구멍뚫림）

;노출部位
@CLOTHES_EXPOSE_702,ARG
;ARG = 노출する部位
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = ヘソ노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 1,2,3,4,6
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비部位
@CLOTHES_PART_702
RETURN 7

;옷の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_702
RETURN 0

;가랑이の구조
;1 = 바지、2 = ビキニ타입、3 = 스커트、4 = 穴開き、5 앞열림、6 開閉가능
@CLOTHES_CROTCH_702
RETURN 4

;장비時の조교効果
@CLOTHES_SOURCE_702,ARG = -1
RETURN 0

;장비時の경험추가
@CLOTHES_EXP_702,ARG = -1
RETURN 0

@CLOTHES_PRICE_702
RETURN 2000

;촉각
;1 = スルー 2 = 上から伝わる 4 = 潜り込ませられる
;ARG 0 = Ｃ 1 = Ｖ 2 = Ａ 3 = 유두 4 = 유방
@CLOTHES_촉각_702,ARG
SELECTCASE ARG
	CASE 4
		RETURN 2
ENDSELECT
