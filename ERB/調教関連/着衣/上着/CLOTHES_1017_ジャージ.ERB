
@CLOTHES_NAME_1017
RESULTS = 저지

;노출部位
@CLOTHES_EXPOSE_1017,ARG
;ARG = 노출する部位
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = ヘソ노출 7 = 음순노출 8 = 둔부노출

SELECTCASE ARG
	CASE 1,4,5,6
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비部位
@CLOTHES_PART_1017
RETURN 10

;옷の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_1017
RETURN 1
;가슴구조
;0 = のっぺり 1 = ボタン 2 = チャック 4 = 上からこんにちわ 8 = たくしあげてこんにちわ 16 = たくし上げてこんにちわ 32 = など、合わせる衣装
@CLOTHES_BREAST_1017
RETURN 2

;가랑이の구조
;1 = 바지、2 = ビキニ타입、3 = 스커트、4 = 穴開き、5 앞열림、6 開閉가능
@CLOTHES_CROTCH_1017
RETURN 0

;장비時の조교効果
@CLOTHES_SOURCE_1017,ARG = -1
RETURN 0

;장비時の경험추가
@CLOTHES_EXP_1017,ARG = -1
		RETURN 0

;장비するための조건
@CLOTHES_PREREQUISITE_1017,ARG
SIF TEQUIP:ARG:옷 == 1
	RETURN 0
RETURN 1

@CLOTHES_PRICE_1017
RETURN 500
