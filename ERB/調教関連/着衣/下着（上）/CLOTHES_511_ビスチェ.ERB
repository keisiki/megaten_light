@CLOTHES_NAME_511
RESULTS = 뷔스티에

;노출部位
@CLOTHES_EXPOSE_511,ARG
;ARG = 노출する部位
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = ヘソ노출 7 = 음순노출 8 = 둔부노출
SELECTCASE ARG
	CASE 4,5,6
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비部位
@CLOTHES_PART_511
RETURN 5

;옷の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_511
RETURN 0

;가슴구조
; 0 開けられない 1 ボタン式 2 チャック式 4 上からこんにちは 8 たくしあげてこんにちは 16 기모노など、合わせる衣装 32 前から差し込めそうな穴 64 입元-가슴元間に페니스入るスペース（ビット演算）
@CLOTHES_BREAST_511
RETURN 12

;가랑이の구조
;1 = 바지、2 = ビキニ타입、3 = 스커트、4 = 穴開き、5 앞열림、6 開閉가능
@CLOTHES_CROTCH_511
RETURN 3

;장비時の조교効果
@CLOTHES_SOURCE_511,ARG = -1
RETURN 0

;장비時の경험추가
@CLOTHES_EXP_511,ARG = -1
RETURN 0

@CLOTHES_PRICE_511
RETURN 500

;촉각
;1 = スルー 2 = 上から伝わる 4 = 潜り込ませられる
;ARG 0 = Ｃ 1 = Ｖ 2 = Ａ 3 = 유두 4 = 유방
@CLOTHES_촉각_511,ARG
SELECTCASE ARG
	CASE 3,4
		RETURN 6
ENDSELECT
