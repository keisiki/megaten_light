				PRINTFORMW 「괴이T트윈즈のマネ。似てるだろ。」
			ELSE
				PRINTFORMW 「ヒットデ〜、ヒットデ〜、…ッテ、誰ガヒトデダッ！」
				PRINTFORML 
				PRINTFORMW 「타천사데카라비아のマネ。似てるだろ。」
			ENDIF
			PRINTFORML
			PRINTFORML [1]似てる
			PRINTFORML [2]いまいち
			$INPUT_LOOP1
			INPUT
			PRINTFORML
			IF RESULT == 1
				;1.似てる
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				SIF TALENT:ARG:눈에띄고싶어함
					LOCAL += RAND:30
				SIF TALENT:ARG:비관적
					LOCAL -= RAND:30
				;실패
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:30
					PRINTFORMW 「褒めるだけは、タダだからなあ。」
					PRINTFORMW 「まじで。」
					RETURN 6
				;성공
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					PRINTFORMW 「そうか、それはよかった。」
					PRINTFORMW 「お代はラヴで結構。なんつって。」
					RETURN 0
				ENDIF
			ELSEIF RESULT == 2
				;2.いまいち
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				SIF TALENT:ARG:냉정
					LOCAL += RAND:30
				SIF TALENT:ARG:현명
					LOCAL += RAND:30
				SIF TALENT:ARG:반항적
					LOCAL -= RAND:30
				;실패
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:50
					PRINTFORMW 「ひじょーに失礼な奴だな、%CSTR:ARG:12%は。」
					PRINTFORMW 「せっかく、%CSTR:ARG:11%自慢の一発芸を見せてやったと言うのに、
					PRINTFORMW 　その態度は無いんじゃないか？　その態度は。」
					RETURN 6
				;성공
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					PRINTFORMW 「そうか。」
					PRINTFORMW 「実は%CSTR:ARG:11%も、ちょっと微妙だと思ってたんだ。」
					PRINTFORMW 「いやいや、まじで。」
					RETURN 0
				ENDIF
			ELSE
				GOTO INPUT_LOOP1
			ENDIF
		CASE 2
			PRINTFORMW 「それにしても、退屈だなあ。」
			PRINTFORMW 「暇過ぎて、死にたくなってくる。いやいや、まじで。」
			PRINTFORMW 「なあ、せっかくだから%CSTR:ARG:12%、%CSTR:ARG:11%を楽しませてくれないか。」
			PRINTFORMW 「ほら、早くしないと本当に暇で死んじまうぞ。」
			PRINTFORML
			PRINTFORML [1]芸を見せる　[2]笑わせてみる
			PRINTFORML [3]무기を抜く
			$INPUT_LOOP2
			INPUT
			PRINTFORML
			IF RESULT == 1
				;1.芸を見せる
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:마력+MAXBASE:PLAYER:운/2)
				;실패
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:30
					IF RAND:2 == 0
						PRINTFORMW 「あ、これが소문に聞く前衛芸術って奴か。」
						PRINTFORMW 「%CSTR:ARG:11%には、何が良いのかサッパリだけどな。」
					ELSE
						PRINTFORMW 「なんだそりゃ。」
						PRINTFORMW 「これなら、三回回ってワンと鳴いてくれたほうが、ずっと良かったぞ。」
					ENDIF
					RETURN 6
				;성공
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					PRINTFORMW 「へえ。」
					PRINTFORMW 「素人の割には、なかなかやるじゃないか。」
					PRINTFORMW 「まじで。」
					RETURN 0
				ENDIF
			ELSEIF RESULT == 2
				;2.笑わせてみる
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				;실패
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:50
					PRINTFORMW ＞%CALLNAME:PLAYER%は気のきいたジョークで笑わせようとした
					PRINTFORMW ＞しかし、%CALLNAME:ARG%には受けなかったようだ……
					PRINTFORMW 「………つまんねえよ、ばーか。」
					RETURN 6
				;성공
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					PRINTFORMW ＞%CALLNAME:PLAYER%は気のきいたジョークで笑わせようとした
					PRINTFORMW 「…っぷ、く、くくく…」
					PRINTFORMW 「く、くっそ、こっ、こんなので…っ、こんなので…ッ…」
					PRINTFORMW 「ははっ、あははははっっ！」
					PRINTFORMW ＞なんと、%CALLNAME:ARG%に受けてしまった！！
					RETURN 0
				ENDIF
			ELSEIF RESULT == 3
				;3.무기を抜く
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				SIF TALENT:ARG:영맹
					LOCAL -= RAND:50
				SIF TALENT:ARG:겁쟁이
					LOCAL += RAND:20
				SIF TALENT:ARG:꿋꿋함
					LOCAL += RAND:30
				;실패
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:30
					PRINTFORMW 「そうか。%CSTR:ARG:12%、%CSTR:ARG:11%と一緒に死にたいのか。」
					PRINTFORMW 「嬉しいよ。じゃあ、%CSTR:ARG:12%から死んでいいよ。」
					RETURN 3
				;성공
				ELSE
					CFLAG:ARG:友好度 += RAND:20
					IF TALENT:ARG:겁쟁이
						PRINTFORMW 「ば、ばか。」
						PRINTFORMW 「人が死ぬって言ったからって、まじで止めをさそうとする奴がいるか。」
						PRINTFORMW 「いいからその物騒なものをしまえ、危ねえだろ。」
					ELSE
						PRINTFORMW 「ばーか、今のは冗談だぞ。まさか、本気にしたのか？」
						PRINTFORMW 「いい奴なのか悪い奴なのか、わかんねえ奴だな、%CSTR:ARG:12%は。」
					ENDIF
					RETURN 0
				ENDIF
			ELSE
				GOTO INPUT_LOOP2
			ENDIF
	ENDSELECT
