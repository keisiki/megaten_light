@KOJO_TALK_PUB305,ARG,ARG:1

SELECTCASE ARG:1
	CASE 1
		;1:레이디・킬러즈
		PRINTFORMW 「あら……」
		PRINTFORMW 「ふふ、どんな小細工をしたのかしら。貴方がとても매력的に見えるわ」
		PRINTFORMW 「けれど、真に매력的な者ならば、小細工を弄さなくても人を虜にできるのよ」
		PRINTFORMW 「%CSTR:ARG:一人称%が身をもって、貴方に教えてア・ゲ・ル%UNICODE(0x2661)%」
	CASE 2
		; 2:おねだり前の逡巡時(아이さつor問答で눈的が決まっている場合)
	PRINTFORML 「貴方悪い人ではないみたいね、さっきのお願い聞いてあげてもいいわ」
	CASE 3
		; 3:交渉の눈的を尋ねる
		SELECTCASE CFLAG:ARG:友好度
			CASE IS < 25
				IF RAND:2 == 0
					PRINTFORMW 「ニンゲンが%CSTR:ARG:11%に何の用かしら？」
				ELSE
					PRINTFORML 「%CSTR:ARG:11%に何か御用？」
				ENDIF
			CASE IS < 50
				PRINTFORMW 「それで……%CSTR:ARG:11%に何か用なの？」
			CASEELSE
				PRINTFORMW 「気分がいいし、%CSTR:ARG:11%に用があるなら聞いてあげるわ」
		ENDSELECT
	CASE 84
		;84:要求前の1文
		PRINT 「そうね……」
	CASE 4
		; 4-6:交渉목적：やらせてくれと言った時の反応
		; 4:取り付く島もなし
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 
				PRINTFORMW 「そう…そうね」
				PRINTFORMW 「조건を出すわ……」
				PRINTFORML 
				PRINTFORMW 「とりあえず死になさい！」
				PRINTFORML 
				PRINTFORMW 激しく拒否された
			CASEELSE
				IF TALENT:ARG:처녀 == 1
					PRINTFORML 「なっ、なにを言い出すの！この変態っ！！」
					PRINTFORMW 激しく拒否された
				ELSE
					PRINTFORML 「잔ネンだけど、アナタ程度に抱かれるほど安いオンナじゃないの」
					PRINTFORMW 激しく拒否された
				ENDIF
		ENDSELECT
	CASE 5
		; 5:交渉次第
		IF TALENT:ARG:처녀
			PRINTFORML 
			PRINTFORMW 「へ？　…あ」
			PRINTFORMW 「そ、そう、%CSTR:ARG:一人称%と…その…えっち…したいの」
			PRINTFORML 
			PRINTFORMW 「ま、まぁ%CSTR:ARG:一人称%も経験豊富だし？　そそ、そうなっちゃうのも仕方ないかしら」
			PRINTFORMW 「アナタの態度次第では考えてあげるわ（キリッ」
			PRINTFORML 
			PRINTFORMW （言えない言えない〜）
			PRINTFORMW （まだ未経験なんて言えるわけ無いわー！！）
			PRINTFORML 
		ELSEIF TALENT:ARG:프라이드높음
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
					PRINTFORMW 「ふ〜ん……」
					PRINTFORMW %CALLNAME:ARG%は%CALLNAME:PLAYER%を値踏みするように眺めた
					PRINTFORML 
					PRINTFORMW 「…で、どのくらい貢いで頂けるのかしら？」
					PRINTFORML 
				CASE 1
					PRINTFORML 
					PRINTFORMW 「アナタに%CSTR:ARG:一人称%の身体は勿体無いと思わないかしら？」
					PRINTFORMW 「でも、まぁ…」
					PRINTFORML 
					PRINTFORMW 「それなりの甲斐性を見せてくれるのなら…考えてあげなくもないかもね、フフッ」
					PRINTFORML 
				CASEELSE
					PRINTFORML 
					PRINTFORMW 「この%CSTR:ARG:一人称%を抱きたいの？」
					PRINTFORMW 「%CSTR:ARG:一人称%の美貌を눈の当たりにしてしまっては仕方のないことかしら…ただ」
					PRINTFORML 
					PRINTFORMW 「%CSTR:ARG:一人称%は安くはないの、お分かりよね？」
					PRINTFORMW 「さぁ…いくら貢いでいただけるのかしら？」
					PRINTFORML 
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 
					PRINTFORMW 「%CSTR:ARG:11%を抱きたいの？ふふふっ%ハート()%」
					PRINTFORMW 「そうね、%CSTR:ARG:二人称%の態度次第…、かしらね♪」
					PRINTFORML
				CASEELSE
					PRINTFORML
					PRINTFORMW 「そうね…アナタよく見るとカワイイし、조건次第で考えてあげてもいいわ」
					PRINTFORML
			ENDSELECT
		ENDIF
		PRINTFORMW 조건次第でヤらせてくれるようだ
		PRINTFORML 
	CASE 6
		; 6:一発おｋ
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 
				PRINTFORMW 「あら♪随分と直情的なアプローチね」
				PRINTFORMW 「いいわよ、た・だ・し…」
				PRINTFORML 
				PRINTFORMW 「溺れちゃダメよ？　うふふっ%ハート()%」
				PRINTFORML 
			CASEELSE
				PRINTFORML 
				PRINTFORML 「フフッ、イイわよ%UNICODE(0x2661)%ほら…こちらにいらっしゃい%UNICODE(0x2661)%…」
				PRINTFORML 
		ENDSELECT
		;負けない…
		SIF TALENT:PLAYER:크고아름다움
			PRINTFORML 「アナタに…%CSTR:ARG:一人称%を満たすことができるかしら？」
		PRINTFORMW 쾌く承諾してくれた
		GOTO ヤらせてもらう・交渉が성공した場合行為中
