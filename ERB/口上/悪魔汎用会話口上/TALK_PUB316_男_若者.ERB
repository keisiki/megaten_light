;=====================================================================
;입調타입：○○の汎用구상＋회화用ファイルです
;=====================================================================

;=====================================================================
;전투時회화および合体時회화の구상テンプレートです。

;個別の악마に対応させたい場合はX1をキャラ番号に
;회화타입別に対応させたい場合はKX1をPUB(회화타입の番号)に
;置き換えてください。

;例）픽시の구상を作りたいならX1⇒501に置き換え
;　　회화타입：女、淑女ならKX1⇒PUB304に置き換え

;知ってると便利そうな表記
;　%CSTR:ARG:X%　X = 11:一人称 12:二人称 20:종족
;　\@ CSTR:ARG:종족명 != "" ? %CSTR:ARG:종족명% # %STR:(ABL:ARG:종족)%
;　┗CSTRに종족명が記載されていればそれを、されていなければABLから종족명を割り出して표시する
;=====================================================================
;사용フラグ
;CFLAG:ARG:800 = 『옷を脱ぐ』を選んだかの판정
;
;=====================================================================
@KOJO_TALK_PUB316,ARG,ARG:1
#DIM LCOUNT

SELECTCASE ARG:1
	CASE 1
		;1:레이디・킬러즈
	PRINTFORMW 「……！？」
	PRINTFORMW 「%CSTR:ARG:一人称%、%CSTR:ARG:二人称%に一눈ぼれしたかもしれない……」
	PRINTFORMW 「いいぜ……ついていってやるよ」
	CASE 2
		; 2:おねだり前の逡巡時(아이さつor問答で눈的が決まっている場合)
	PRINTFORML 「どうすっかなあ」
	CASE 3
		; 3:交渉の눈的を尋ねる
		SELECTCASE CFLAG:ARG:友好度
			CASE IS < 25
				;友好度低い
				PRINTFORMW 「で、%CSTR:ARG:11%に何の用だ？」
			CASE IS < 50
				;友好度普通
				PRINTFORMW 「%CSTR:ARG:11%に何か用があるんだろ？」
			CASEELSE
				;友好度高い
				PRINTFORML 「%CSTR:ARG:11%に頼みがあるんだろ？」
				PRINTFORMW 「聞いてやるから、言ってみな」
		ENDSELECT
	CASE 84
		;84:要求前の1文
		PRINT 「まずは…」
	CASE 4
		; 4-6:交渉목적：やらせてくれと言った時の反応
		; 4:取り付く島もなし
			PRINTFORMW 激しく拒否された
	CASE 5
		; 5:交渉次第
		PRINTFORMW 조건次第でヤらせてくれるようだ
	CASE 6
		; 6:一発おｋ
		PRINTFORMW 「%CSTR:ARG:11%を誘ってんのか？」
		PRINTFORMW 「それじゃ、遠慮なく｣
	CASE 7
		; 7-9:交渉목적：犯してほしいと言った時の反応
		; 7:取り付く島もなし
			PRINTFORMW 激しく拒否された
	CASE 8
		; 8:交渉次第
		PRINTFORMW 조건次第で犯してくれるようだ
	CASE 9
		; 9:一発おｋ
		CALL MESSAGE_TALK
		PRINTFORML 
		IF TFLAG:메시지패턴 == 1
			;스커트걷어올림
			;ノーパン
			IF TEQUIP:PLAYER:음순노출 == -1
				IF STAIN:PLAYER:3 & 4
					;既に정액が…
					PRINTFORMW 「ハハハ、何だお前、マンコからザーメン垂らしてんじゃねえか」
					PRINTFORMW 「犯される為にこんな所うろついてんじゃねえのか？ ったく、とんだ음란\@TALENT:PLAYER:남자 == 0 ? 女 # ヤロウ\@じゃねえか」
					SIF TALENT:PLAYER:연모 || TALENT:PLAYER:친애
						PRINTFORMW 「ソッチの\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@もかわいそうになぁ、こんな変態が恋人なんてよ」
					PRINTFORMW 「それじゃ%CSTR:ARG:一人称%も中に出してやるよ、喜べよ変態\@TALENT:PLAYER:남자 == 0 ? 女 # ヤロウ\@」
				ELSE
					PRINTFORMW 「おいおい、스커트履いて下は何も履いてねえなんて、オマエは何しにこんなとこに来てんだよ」
					PRINTFORMW 「犯される為にうろつくなんてエロイ\@TALENT:PLAYER:남자 == 0 ? 女 # ヤロウ\@じゃねえか　ま、遠慮なく頂くぜ」
				ENDIF
			;エロ장비をしている
			ELSEIF IS_EROEQUIP_F(PLAYER)
					PRINTFORMW 「エロい鎧来てうろついているんだからよぉ、誰のでもいいからチンポが欲しかったんだろう？」
					IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애 
						PRINTFORMW 「ん？\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@に見せたかったのかぁ？」
						PRINTFORMW 「残念！\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@の前で知らない男の아귀産みな！」
					ELSE
						PRINTFORMW 「安心しな。ちゃーんと%CSTR:ARG:11%が孕ませてやるからよ！」
					ENDIF
			;パンツか何かはいてる
			ELSE
				PRINTFORMW 「ハハ、いい格好じゃねえか」
				IF HAVE_PENIS(ARG)
					PRINTFORMW %CALLNAME:ARG%は自分の一物を曝け出すと、스커트をたくし上げたままの%CALLNAME:PLAYER%の股座に擦り付けた。
				ELSE
					PRINTFORMW %CALLNAME:ARG%は스커트をたくし上げたままの%CALLNAME:PLAYER%の股座を上下するように擦り始めた。
				ENDIF
				PRINTFORMW 「それじゃお望みどおり…そうだな、その格好のまま犯してやるよ｣
			ENDIF
		ELSEIF TFLAG:메시지패턴 == 2
			;Ｍ字開다리
			IF RAND:2 == 0
				IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애 
					PRINTFORMW 「オ이오イ、\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@の前で%CSTR:ARG:11%をＭ字で유혹かあ？」
					PRINTFORMW 「ハハッ、イイぜ。腹ん中タプタプになるまで犯してやるよ」
					PRINTFORMW 「\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@の눈の前で%CSTR:ARG:11%の아귀孕めや、ヒャハハハハッ」
				ELSE
					PRINTFORMW 「ヘヘ、物欲しそうな面しやがって、そんなに%CSTR:ARG:11%のを咥えたいのか？」
					PRINTFORMW 「それじゃご希望どおり、腰がガクガクになるまで付き捲ってやるよ」
				ENDIF
					PRINTFORMW %CALLNAME:ARG%は%CALLNAME:PLAYER%に覆いかぶさった
			;エロ장비をしている
			ELSEIF IS_EROEQUIP_F(PLAYER)
					PRINTFORMW 「エロい鎧来て股開くなんて、たいしたアバズレだなぁ？オイ」
					IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애 
						PRINTFORMW 「ん？\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@に見せたかったのかぁ？」
						PRINTFORMW 「ならよぉ、악마のチンポで\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@の予約済みマンコぶっ壊してやるぜ！」
					ELSE
						PRINTFORMW 「ほーら鎧を外しな。大事なとこがよーく見えるようによ！」
					ENDIF
			ELSE
				IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애 
					PRINTFORMW 「へへ、そんなエロイカッコされちまうと、メチャクチャにしたくなるじゃねえか」
					PRINTFORMW 「しばらくヤってねえからな…ドロッドロのザーメン、テメエのマンコに流し込んでやるか」
					PRINTFORMW 「たっぷり%CSTR:ARG:11%の味を憶え込ませてやるからよ」
					PRINTFORMW 「自分から%CSTR:ARG:11%の肉변기に志願したくなるようにな」
				ELSE
					PRINTFORMW 「いい眺めだなあ、オイ」
					PRINTFORMW 「あんまり焦らすのも可哀相だし、さっそく犯らせてもらうか」
				ENDIF
					PRINTFORMW %CALLNAME:ARG%は%CALLNAME:PLAYER%に覆いかぶさった
			ENDIF
		ELSEIF TFLAG:메시지패턴 == 3
			;おっぱい
			;男の人ってみんなおっぱい好きなんですよね？
			IF RAND:2 == 0
				IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애 
					PRINTFORMW 「チクビ弄っただけでイっちまうなんてどんだけスケベなんだよ」
					PRINTFORMW 「摘んだだけでどんな男にも股開くってか？便利な肉변기だぜ」
					PRINTFORMW 「オイ、アンタこんな\@TALENT:PLAYER:남자 == 0 ? オンナ # 남자\@別れたほうが良いんじゃねえか？」
					PRINTFORMW 「今なら%CSTR:ARG:11%がタダで引き取ってやるぜ、ヒヒヒヒ」
				ELSE
					PRINTFORMW 「へへへ、やっぱオッパイはいいモンだよなあ」
					PRINTFORMW 「%CSTR:ARG:11%の気が済むまで弄りまくってやるぜ」
				ENDIF
					PRINTFORMW %CALLNAME:ARG%は%CALLNAME:PLAYER%の가슴を乱暴に掴みながら、입内に舌を入れた
			ELSE
				IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애 
					PRINTFORMW 「エッロイカラダしてんな？オイッ」
					PRINTFORMW 「毎日、こいつとヤれるなんて\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@が羨ましいぜ」
					PRINTFORMW 「まっ、今は%CSTR:ARG:11%の팔の中にあるんだけどな、ヒヒヒ」
					PRINTFORMW 「おっぱいもこーんなふうに揉み放題だぜ、ヒャハハ」
				ELSE
					PRINTFORMW 「へへ、もう先っちょが尖がってるぜ」
					PRINTFORMW 「摘んでやるよ、ほれっ」
					PRINTFORMW 「イっちまったかあ？ヒヒヒヒ」
				ENDIF
					PRINTFORMW %CALLNAME:ARG%は%CALLNAME:PLAYER%の가슴を乱暴に掴みながら、입内に舌を入れた
			ENDIF
		ELSEIF TFLAG:메시지패턴 == 5
			;스커트걷어올림・パンツずらし
			PRINTFORMW 「おっ…ハハ、いいぞいいぞ」
			PRINTFORMW 「自分から\@TALENT:PLAYER:남자 == 0 ? # ケツ\@マンコおっぴろげるような음란\@TALENT:PLAYER:남자 == 0 ? 女 # ヤロウ\@には%CSTR:ARG:一人称%がコイツをくれてやるよ」
			
		ELSEIF TFLAG:메시지패턴 == 4
			;四つん這い誘い
			IF RAND:2 == 0
				IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애
					PRINTFORMW 「へっへっへっ、\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@の눈の前で他の남자にケツ出すたぁ、エッロイ\@TALENT:PLAYER:남자 == 0 ? 女 # ヤロウ\@じゃねえか」
					PRINTFORMW 「たまんねぇな、オイ！しっかり中出ししてやるからなぁ」
					PRINTFORMW 「そこのオマエ！ テメエはコイツが%CSTR:ARG:11%に抱かれるところを指くわえて眺めてな」
				ELSE
					PRINTFORMW 「へへ…いい格好じゃねえか。エッロイケツしてやがる」
				ENDIF
			ELSE
				IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애
					PRINTFORMW 「くくくっ、他人の\@TALENT:PLAYER:남자 == 0 ? 女 # モノ\@に손ぇ出すのは興奮するぜ」
					PRINTFORMW 「そんな眼で見なくてもコイツの\@TALENT:PLAYER:남자 == 0 ? マンコ # ケツ\@ん中、%CSTR:ARG:11%のザーメンでいっぱいになったら返してやるよ」
					PRINTFORMW 「まあ、そん時にはコイツがオマエん所に戻りたがらなくなってるかもしんねーけどな！ヒャハハハハ！！」
				ELSE
					PRINTFORMW 「犯される準備は万端ってか？ ケケ、ヨガり過ぎてイキ狂うんじゃねえぞ？」
				ENDIF
			ENDIF
		ELSEIF TFLAG:메시지패턴 == 7
			;펠라치오
			IF RAND:2 == 0
				IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애 
					PRINTFORMW 「おおっ、けっこう巧えじゃねぇか」
					PRINTFORMW 「いつもこうしてチンポしゃぶってんのかよっっと」
					PRINTFORMW 「オ이오イ%CSTR:ARG:二人称%、%CSTR:ARG:11%のザーメン喉鳴らして飲んでやがるぜ」
					PRINTFORMW 「そんなにザーメン飲むのが好きならこっちの입にも飲ましてやるよ」
					PRINTFORMW 「腹パンパンになるまでなぁ、ヒャハハハ」
				ELSE
					PRINTFORMW 「へへ、うまそうにしゃぶるじゃねえか」
					PRINTFORMW 「オラッ、다리開けっ！」
					PRINTFORMW 「次はこっちを味見してやるよ」
				ENDIF
				PRINTFORMW %CALLNAME:ARG%は%CALLNAME:PLAYER%を押し倒し、涎と정액にまみれた페니스を挿入した
			ELSE

				IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애 
					PRINTFORMW 「オ이오イ、いいのかぁ？\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@が見てるぜぇ」
					PRINTFORMW 「イクぞ、全部飲み干せや」
					PRINTFORMW 「どうだ？\@TALENT:MASTER:남자 == 0 ? 彼女 # 彼氏\@の前で飲むザーメンの味は？」
					PRINTFORMW 「…なぁに옷直してんだよ、本番はこれからだろぉ！！」
					PRINTFORMW 「今度は%CSTR:ARG:二人称%の腹ん中に出してやるよ、ヒャハハハ」
				ELSE
					PRINTFORMW 「ずいぶん손馴れてんなぁ？マジ気持ちいいぜ」
					PRINTFORMW 「やべえな、マジで\@TALENT:MASTER:남자 == 0 ? 便女 # 변기\@にしたくなってきたぜ」
				ENDIF
				PRINTFORMW %CALLNAME:ARG%は%CALLNAME:PLAYER%を押し倒し、涎と정액にまみれた페니스を挿入した
			ENDIF
		;魅了상태
		ELSEIF TFLAG:메시지패턴 == 8
			IF TALENT:PLAYER:연모 || TALENT:PLAYER:친애
				PRINTFORML 
				PRINTFORMW 「あぁっ？『%CALLNAME:MASTER%』〜？」
				PRINTFORMW 「あーあー、コイツの\@TALENT:PLAYER:아내 ? ダンナ # カレシ\@の事かぁ？」
				PRINTFORML 
				PRINTFORMW 「コイツ、\@TALENT:PLAYER:아내 ? ダンナ # カレシ\@に抱かれてるって思ってやがるのかよ！　マジうけるぜ〜！！」
				PRINTFORML 
			ELSE
				PRINTFORMW 「%CSTR:ARG:11%を誘ってんのか？」
				PRINTFORMW 「それじゃ、遠慮なく｣
			ENDIF
		ELSE
			PRINTFORMW 「%CSTR:ARG:11%を誘ってんのか？」
			PRINTFORMW 「それじゃ、遠慮なく｣
		ENDIF
		PRINTFORML 
	CASE 10
		;10:要求額を提示：￥
		PRINTFORML 「￥{TFLAG:TALK用要求値}だな」
	CASE 11
		;11-13:￥を払おうとしてお金が발りない
		;11:別のものを要求する
			PRINTFORMW 「貧乏なんだな」
				PRINTFORMW 「しょうがねえな、他の物で我慢してやるよ」
	CASE 85
		;85:恵まれる
		PRINTFORMW 「貧乏なんだな」
		PRINTFORMW 「すくねえけどよ、コレとっときな」
	CASE 12
		;12:交渉실패。立ち去る
			PRINTFORMW 「貧乏なんだな」
