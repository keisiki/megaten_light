						ELSEIF TALENT:ARG:프라이드높음
							PRINTFORML 「%CSTR:ARG:12%がそうなのか！」
							PRINTFORMW 「大将首だ、逃がしはしねえ！」
							RETURN 3
						ELSE
							PRINTFORMW 「おいおい%CSTR:ARG:12%、머리大丈남편か？」
							RETURN 6
						ENDIF
					ELSE
						CFLAG:ARG:友好度 += RAND:30
						;성공
						IF BASE:ARG:LV > BASE:PLAYER:LV
							CFLAG:ARG:友好度 -= RAND:20
							PRINTFORMW 「%CSTR:ARG:12%がそうだって言うのか？」
							PRINTFORMW 「とてもそうは見えねえが…そうなのか」
							RETURN 0
						ELSEIF TALENT:ARG:영맹
							PRINTFORML 「道理で強そうだと思ったぜ」
							PRINTFORMW 「いちど손合せしてほしいもんだな、おい」
							RETURN 0
						ELSEIF TALENT:ARG:교활
							PRINTFORML 「%CSTR:ARG:12%がそうだってえこたぁ…儲かっているんだろ？」
							PRINTFORMW 「話を聞いてやろうか？へっへっへ…」
							CFLAG:ARG:友好度 += RAND:10
							RETURN 0
						ELSEIF TALENT:ARG:겁쟁이
							CFLAG:ARG:友好度 += RAND:20
							PRINTFORML 「%CSTR:ARG:12%がそうだってのか、」
							PRINTFORMW 「そうとは気づかないままの数々の無礼　許してくれ」
							RETURN 0
						ELSEIF TALENT:ARG:우애
							CFLAG:ARG:友好度 += RAND:15
							PRINTFORML 「인간の身で악마と張り合うなんてすげえな」
							PRINTFORMW 「そういう無茶やっているとケガも多いだろう、大丈남편か？」
							RETURN 0
						ELSE
							PRINTFORML 「%CSTR:ARG:12%がそうだったのか」
							PRINTFORMW 「악마ってのはな、強いやつが大好きなのさ」
							RETURN 0
						ENDIF
					ENDIF
				ELSE
					GOTO INPUT_LOOP_A5
				ENDIF
			ENDIF
