@KOJO_TALK_PUB304,ARG,ARG:1

SELECTCASE ARG:1
	CASE 1
		;1:레이디・킬러즈
	IF RAND:2 == 0
		PRINTFORMW 「……！」
		PRINTFORMW 「ねえアンタ、운命って信じる？」
		PRINTFORMW 「%CSTR:ARG:一人称%はたった今から信じたくなったわ」
		PRINTFORMW 「アンタ、%CSTR:ARG:一人称%を連れてってみない？大丈남편、損はさせないよ」
	ELSE
		PRINTFORMW 「……！」
		PRINTFORMW 「%CSTR:ARG:一人称%、당신の奴隷よ」
	ENDIF
	CASE 2
		; 2:おねだり前の逡巡時(아이さつor問答で눈的が決まっている場合)
		PRINTFORML 「うん、中々良いヤツみたいだし、アンタのお願い聞いてあげなくもないよ」
	CASE 3
		; 3:交渉の눈的を尋ねる
		SELECTCASE CFLAG:ARG:友好度
			CASE IS < 25
				PRINTFORMW 「そんでなあに、ニンゲン？」
				PRINTFORML 「%CSTR:ARG:11%に何か用？」
			CASE IS < 50
				PRINTFORMW 「それで……%CSTR:ARG:11%に何か用なの？」
			CASEELSE
				PRINTFORMW 「気分がいいし、%CSTR:ARG:11%に用があるなら聞いてあげるよ」
		ENDSELECT
	CASE 84
		;84:要求前の1文
		PRINT 「それじゃあ……」
	CASE 4
		; 4-6:交渉목적：やらせてくれと言った時の反応
		; 4:取り付く島もなし
			PRINTFORMW 「っ！？このヘンタイッ！！死んじゃえ！！」
	CASE 5
		; 5:交渉次第
		IF TALENT:ARG:처녀
			PRINTFORML 
			PRINTFORMW 「なっ！？なにいって…っ！……ふ〜ん♪」
			PRINTFORML 
			PRINTFORMW 「いいよ〜、%CSTR:ARG:一人称%の出す조건をクリアできたらね〜」
			PRINTFORML 
		ELSEIF TALENT:ARG:츤데레
			PRINTFORML 
			PRINTFORMW 「はぁっ！？な、なに言ってんのよ！バッカじゃないの！？」
			PRINTFORMW 「……で、でも…どうしてもって言うんなら、ニンゲンの態度次第で…考えなくもないわよ」
			PRINTFORML 
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
					PRINTFORMW 「えー？　ん〜…%CSTR:ARG:一人称%、お小遣いほしいんだぁ♪」
					PRINTFORML 
				CASE 1
					PRINTFORML 
					PRINTFORMW 「えー、%CSTR:ARG:一人称%って、そんなに軽く見える〜？」
					PRINTFORMW 「流石にタダじゃ嫌だよ？　えへへ♪」
					PRINTFORML 
				CASEELSE
					PRINTFORML 
					PRINTFORMW 「え〜？%CSTR:ARG:一人称%とシたいの？」
					PRINTFORMW 「そうだな〜、조건次第なら考えてあげてもイイよ♪」
					PRINTFORML 
			ENDSELECT
		ENDIF
		PRINTFORMW 조건次第でヤらせてくれるようだ
	CASE 6
		; 6:一発おｋ
		;처녀
		IF TALENT:ARG:처녀
			PRINTFORML 
			PRINTFORMW 「っ！？　…………いいよ」
			PRINTFORMW 「당신、ちょっと好みだし…その…」
			PRINTFORML 
			PRINTFORMW 「%CSTR:ARG:一人称%もそろそろ『卒業』したかったし……%ハート()%」
			PRINTFORML 
		;通常
		ELSE
			$ヤラせて欲しい一発おｋ_LOOP
			SELECTCASE RAND:2
				CASE 0
					SIF !TALENT:ARG:프라이드높음
						GOTO ヤラせて欲しい一発おｋ_LOOP
					PRINTFORML 
					PRINTFORMW 「はぁ！？なに言ってんの？머리オカシイんじゃないの、このニンゲン」
					PRINTFORMW 「…でもまあ、%CSTR:ARG:一人称%のカラダを見てサカっちゃうのはしょうがないし？」
					PRINTFORMW 「お情けでヤラせてあげるわよ、ヘンタイ♪」
					PRINTFORMW 쾌く（？）承諾してくれた
					PRINTFORML 
				CASEELSE
					SELECTCASE RAND:5
						CASE 0
							PRINTFORML 
							PRINTFORMW 「%ハート()%　いいよぉ、%CSTR:ARG:一人称%もちょっとイイナ〜って思ってたの%ハート()%」
							PRINTFORMW 「いっしょに愉しもうね%ハート()%」
							PRINTFORML 
							PRINTFORMW 쾌く承諾してくれた
							PRINTFORML 
						CASE 1
							PRINTFORML 
							PRINTFORMW 「ふぅ〜ん？　うふふっ♪」
							PRINTFORMW 「すっごいオスの匂い%ハート()%　さぞ激しいんでしょうね〜♪」
							PRINTFORML 
							PRINTFORMW 「期待、してるからねぇ%ハート()%」
							PRINTFORML 
							PRINTFORMW 쾌く承諾してくれた
							PRINTFORML 
						CASE 2
							PRINTFORML 
							PRINTFORMW 「きゃん♪　『ヤラせろ』だなんて〜」
							PRINTFORMW 「ちょっとお腹の奥にキュン☆ってきちゃった♪」
							PRINTFORML 
							PRINTFORMW 쾌く承諾してくれた
							PRINTFORML 
						CASE 3
							PRINTFORML 
							PRINTFORMW 「え〜〜？　アナタもスキね〜〜？」
							PRINTFORMW 「な〜んてね、%CSTR:ARG:一人称%も嫌いじゃないんだけど♪」
							PRINTFORML 
							PRINTFORMW 쾌く承諾してくれた
							PRINTFORML 
						CASEELSE
							PRINTFORML 
							PRINTFORML 「ふ〜ん…악마とヤリたいなんて、ニンゲンってヘンタ〜イ？」
							PRINTFORMW 「いいわよ、ヤラせてあげる♪　そのかわり満발させてくれなかったらサ…」
							PRINTFORMW 「モグからね！イロイロと…ふふふっ♪」
							PRINTFORMW 쾌く（？）承諾してくれた
							PRINTFORML 
					ENDSELECT
			ENDSELECT
			;負けない…
			SIF TALENT:PLAYER:크고아름다움
				PRINTFORML 「でもアナタに…%CSTR:ARG:一人称%を満たすことができるかな？」
		ENDIF
		GOTO ヤらせてもらう・交渉が성공した場合行為中
	CASE 7
		; 7-9:交渉목적：やらせてくれと言った時の反応
		; 7:取り付く島もなし
			PRINTFORMW 激しく拒否された
	CASE 8
		; 8:交渉次第
		PRINTFORMW 조건次第で犯してくれるようだ
	CASE 9
		; 9:一発おｋ
		PRINTFORMW 쾌く承諾してくれた
	CASE 10
		;10:要求額を提示：￥
		PRINTFORML 「￥{TFLAG:TALK用要求値}ちょうだい」
	CASE 11
		;11-13:￥を払おうとしてお金が발りない
		;11:別のものを要求する
		PRINTFORMW 「お金がないの？」
		PRINTFORMW 「仕方ないなあ、別のもので我慢してあげるわ」
	CASE 85
		;85:恵まれる
		PRINTFORMW 「お金がないの？」
		PRINTFORML 「仕方ないなあ、このくらい持っておきなさいよね」
	CASE 12
		;12:交渉실패。立ち去る
		PRINTFORMW 「お金がないの？」
		PRINTFORMW 「何かシラけちゃったね」
		PRINTFORMW 「%CSTR:ARG:11%もう帰っていい？」
	CASE 13
		;13:交渉실패。적対する
		PRINTFORMW 「お金がないの？」
		PRINTFORMW 「それっぽっちも持ってないのに話しかけないでよ！」
	CASE 14
		;14-16:￥を支払った
		;14:払ったのに逃げられる
		PRINTFORMW 「ありがとう！」
	CASE 15
		;15:さらに要求される
		PRINTFORML 「まだだよ あせっちゃダメ」
	CASE 16
		;16:交渉성공
		IF FLAG:交渉목적 > 3
			;CASE 88にまとめて설정
		ELSE
			PRINTFORMW 「ありがとう！」
		ENDIF
	CASE 17
		;17-19:￥を支払うのを断った::
		;17:交渉실패。奇襲する
				PRINTFORMW 「アタシには払いたくないっての？」
				PRINTFORMW 「ああもう、どうして인간ってこうムカつくんだろ」
				PRINTFORMW 「もういいや、死んでよ。今すぐ！」
	CASE 18
		;18:交渉실패。적対する
				PRINTFORMW 「アタシには払いたくないっての？」
				PRINTFORMW 「話の分かるヤツかも、なんてちょっとでも思ったのが間違いだったわ」
				PRINTFORMW 「もーやめ、アンタと話す事なんて何一つないよ」
	CASE 19
		;19:他のものを要求する:
				PRINTFORMW 「そう、じゃあ別のにしよっかな」
	CASE 20
		;20:要求額を提示：ＭＡＧ
		PRINTFORML 「{TFLAG:TALK用要求値}MAGちょうだい」
		PRINTFORML
	CASE 21
		;21-23:ＭＡＧを支払おうとしたが발りない
		;21:別のものを要求する
		PRINTFORMW 「ＭＡＧが발りないの？」
		PRINTFORMW 「仕方ないなあ、別のもので我慢するよ」
	CASE 86
		;86:恵まれる
		PRINTFORMW 「ＭＡＧが발りないの？」
		PRINTFORML 「仕方ないなあ、このくらい持っておきなさいよね」
		
	CASE 22
		;22:交渉실패。立ち去る
		PRINTFORMW 「ＭＡＧが발りないの？」
		PRINTFORMW 「何かシラけちゃったね」
		PRINTFORMW 「%CSTR:ARG:11%もう帰っていい？」
	CASE 23
		;23:交渉실패。적対する
		PRINTFORMW 「ＭＡＧが발りないの？」
		PRINTFORMW 「それっぽっちも持ってないのに話しかけないでよ！」
	CASE 24
		;24-26:ＭＡＧを支払った
		;24:払ったのに逃げられる
		PRINTFORMW 「ありがとう！」
	CASE 25
		;25:さらに要求される
		PRINTFORML 「まだだよ あせっちゃダメ」
	CASE 26
		;26:交渉성공
		IF FLAG:交渉목적 > 3
			;CASE 88にまとめて설정
		ELSE
			PRINTFORMW 「ありがとう！」
		ENDIF
	CASE 27
		;27-29:ＭＡＧを支払うのを断った
		;27:交渉실패。奇襲する
				PRINTFORMW 「アタシには払いたくないっての？」
				PRINTFORMW 「ああもう、どうして인간ってこうムカつくんだろ」
				PRINTFORMW 「もういいや、死んでよ。今すぐ！」
	CASE 28
		;28:交渉실패。적対する
				PRINTFORMW 「アタシには払いたくないっての？」
				PRINTFORMW 「話の分かるヤツかも、なんてちょっとでも思ったのが間違いだったわ」
				PRINTFORMW 「もーやめ、アンタと話す事なんて何一つないよ」
	CASE 29
		;29:他のものを要求する
		PRINTFORMW 「そう、じゃあ別のにしよっかな」
	CASE 30
		;30:마석を要求
		PRINTFORML 「마석くれない？」
		PRINTFORML
	CASE 31
		;31-33:마석を渡そうとしたが持っていない
		;31:別のものを要求する
		PRINTFORMW 「마석もないの？」
		PRINTFORMW 「仕方ないなあ、別のもので我慢してあげる」
	CASE 87
		;87:恵まれる
		PRINTFORMW 「마석もないの？」
		PRINTFORMW 「서머너なら마석の一つくらい持ってなよね。ホラ！」
	CASE 32
		;32:交渉실패。立ち去る
		PRINTFORMW 「마석もない서머너と交渉してもねえ……」
		PRINTFORMW 「%CSTR:ARG:11%もう帰るから」
	CASE 33
		;33:交渉실패。적対する
		PRINTFORMW 「마석も持ってないの？」
		PRINTFORMW 「마석もないのに악마と交渉するなんて、アンタ舐めてんじゃないの？」
	CASE 34
		;34-36:마석を渡した
		;34:払ったのに逃げられる
		PRINTFORMW 「ありがとう！」
	CASE 35
		;35:さらに要求される
		PRINTFORML 「まだよ、あせっちゃダメ」
	CASE 36
		;36:交渉성공
		IF FLAG:交渉목적 > 3
			;CASE 88にまとめて설정
		ELSE
			PRINTFORMW 「ありがとう！」
		ENDIF
	CASE 37
		;37-39:마석を渡すのを断った
		;37:交渉실패。奇襲する
				PRINTFORMW 「アタシには渡したくないっての？」
				PRINTFORMW 「ああもう、どうして인간ってこうムカつくんだろ」
				PRINTFORMW 「もういいや、死んでよ。今すぐ！」
	CASE 38
		;38:交渉실패。적対する
				PRINTFORMW 「アタシには渡したくないっての？」
				PRINTFORMW 「話の分かるヤツかも、なんてちょっとでも思ったのが間違いだったわ」
				PRINTFORMW 「もーやめ、アンタと話す事なんて何一つないよ」
	CASE 39
		;39:他のものを要求する
				PRINTFORMW 「そう、じゃあ別のにしよっかな」
	CASE 40
		;40:異性との성교渉を要求
		PRINTFORML 
		PRINTFORML 「最近、ご無沙汰でさ…相손、してもらえない%ハート()%」
		PRINTFORML \@TALENT:ARG:남자 ?女#男\@を要求されました
		PRINTFORML 
	CASE 41
		;41:異性を宛がってもらった時の反応
		$異性を宛がってもらった時の反応_LOOP
		IF TALENT:(捧げたキャラ()):남자
			;처녀
			IF TALENT:ARG:처녀
				PRINTFORML 
				PRINTFORMW （わーわーどうしよう！　じょ、冗談のつもりだったのに〜！！）
				PRINTFORMW （でもでも、このチャンスを逃したら次はないかもだし…）
				PRINTFORML 
				PRINTFORMW （…それにけっこう타입だし……うん！がんばる！！）
				PRINTFORML 
			ELSE
				SELECTCASE RAND:2
					CASE 0
						SIF !TALENT:ARG:거유 || !TALENT:ARG:폭유
							GOTO 異性を宛がってもらった時の反応_LOOP
						PRINTFORML 
						PRINTFORMW 「えいっ！　가슴で挟んであげる♪」
						PRINTFORMW 「さっきから見てたの気付いてたんだからね〜♪」
						PRINTFORML 
					CASEELSE
						SELECTCASE RAND:4
							CASE 0
								PRINTFORML 
								PRINTFORMW 「わわわっ、カチカチ〜♪　これは期待できそうだね%ハート()%」
								PRINTFORML 
							CASE 1
								PRINTFORML 
								PRINTFORMW 「わっ%ハート()%……この前の서머너よりもおっきいかも%ハート()%」
								PRINTFORML 
							CASE 2
								PRINTFORML 
								PRINTFORMW 「それじゃあ、まずは……ちゅっ%ハート()%」
								PRINTFORMW 「んっ…はぁ……えへへ♪　ちょっと恥ずかしいかも」
								PRINTFORML 
							CASEELSE
								PRINTFORML 
								PRINTFORMW 「わっ♪　おっきい〜%ハート()%」
								PRINTFORMW 「それじゃ、いっただっきま〜す%ハート()%」
								PRINTFORML 
						ENDSELECT
				ENDSELECT
			ENDIF
		ENDIF
		CALL MESSAGE_TALK
		IF TALENT:(捧げたキャラ()):남자
			;처녀
			IF TALENT:ARG:처녀
				PRINTFORML 
				PRINTFORMW 「いっ、痛いっ！？　もっと、優しく、してよぉ…」
				PRINTFORMW 「%CSTR:ARG:一人称%…、『初めて』なんだからぁ…%ハート()%」
				PRINTFORML 
