						PRINTFORMW 「あはっ♪　出てるよ〜♪」
						PRINTFORMW 「%CSTR:ARG:一人称%の中でどぴゅどぴゅって%ハート()%」
						PRINTFORML 
					CASE 2
						PRINTFORML 
						PRINTFORMW 「んふっ♪　んふふふふ〜♪」
						PRINTFORMW 「腰、浮いちゃってるよ〜？　可愛い〜んだから〜%ハート()%」
						PRINTFORML 
					CASEELSE
						PRINTFORML 
						PRINTFORMW 「んっ%ハート()%　スゴイ♪　ホントに奥まで%ハート()%」
						PRINTFORMW 「オネダリして正解だったわね〜♪　あんっ%ハート()%」
						PRINTFORML 
				ENDSELECT
			ENDIF
		ENDIF
	CASE 42
		;42-46:성교渉が終わった後
		;42:敗北してる場合ここでゲームオーバー
	CASE 43
		;43:話しかけたキャラが倒れている
	CASE 44
		;44:ヤらせたのに逃げられる
		PRINTFORMW %CALLNAME:ARG%に逃げられた
	CASE 45
		;45:満발してない
		PRINTFORML %CALLNAME:ARG%はまだ満발していないようだ
	CASE 46
		;46:満발
		;男をあてがった場合
		IF TALENT:(捧げたキャラ()):남자
			;처녀の場合
			IF TALENT:ARG:처녀
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 
						PRINTFORMW 「あぅ〜…まだなんか入ってる感じがするぅ…」
						PRINTFORMW 「えっ？　べ、べつにイヤじゃないよ……」
