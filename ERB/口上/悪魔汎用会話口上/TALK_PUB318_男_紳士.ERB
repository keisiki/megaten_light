#DIM 発情キャラ番号, 7
#DIM 発情要求キャラ番号
発情人数 = 0

SELECTCASE CFLAG:ARG:友好度
	CASE IS > 25
		PRINTFORML 「ふむ、ちょうど暇だったところでしてね」
		PRINTFORMW 「喜んでお話を伺わせてもらうとしましょう」
	CASE IS > -25
		;最初の挨拶
		IF TALENT:ARG:잔학
			PRINTFORML 「인간風情が何の用ですか」
			PRINTFORML 「バラバラにされて泣き叫んでくれるのでしょうか？」
		ELSE
			PRINTFORML 「%CSTR:ARG:11%に話があるのですか？」
			PRINTFORMW 「お伺いしましょう」
		ENDIF
	CASEELSE
		PRINTFORML 「인간風情が話…ですか」
		PRINTFORMW 「まぁ一応聞いてさしあげましょう」
ENDSELECT
PRINTFORML

;発情キャラの躾け회화
;発情キャラが居るのを嗅ぎ付け、躾を提案する
;任せた場合は真面눈に躾けられると発情が0に、ただヤラれると発情のまま
;パ−ティ内に발정중のキャラが居る場合に１０％の확률で発生
IF RAND:10 == 0
	FOR LOCAL, 1, 7
		;存在しないか死んでいるならスキップ
		SIF POS(LOCAL) < 0 || GET_STATE(CFLAG:(POS(LOCAL)):ステート) == "DYING"
		CONTINUE
		;発情以外は弾く
		SIF 위험일(POS(LOCAL)) <= 1
			CONTINUE
		発情キャラ番号:発情人数 = POS(LOCAL)
		発情人数 ++
	NEXT
	;一人も居ない場合はスキップ
	SIF 発情人数 == 0
		GOTO SKIP_発情キャラ要求
	;要求キャラの選定
	発情要求キャラ番号 = 発情キャラ番号:(RAND:発情人数)
	
	PRINTFORML 「おや？なにやら匂いますね」
	PRINTFORMW 「発情したメスのいやらしい香りが、そちらから漂ってきますよ」
	PRINTFORML 
	PRINTFORMW %CALLNAME:ARG%は%CALLNAME:発情要求キャラ番号%を指差した
	PRINTFORML 
	PRINTFORML 「あのような物欲しそうな눈で、こちらを見られていてはたまりません」
	PRINTFORMW 「%CSTR:ARG:11%が躾けて差し上げましょうか？」
	PRINTFORML 
	PRINTFORML [1]任せる　　[2]断る
	PRINTFORML 
	CALL INPUTINT(1, 2)
	SELECTCASE RESULT
		;任せる
		CASE 1
			PRINTFORML 
			PRINTFORMW 「わかりました、しっかりと躾けて差し上げましょう」
			PRINTFORML 
			PRINTFORMW ・・・
			PRINTFORML 
			PRINTFORMW ・・
			PRINTFORML 
			PRINTFORMW ・
			PRINTFORML 
			PRINTFORMW 

			;真面눈に躾けられる확률は月齢や相손の욕망などによって変動する（１０〜３０％）
			;真面눈に躾けられる
			IF RAND:100 < 30/(1+仮_異性を要求するか？(ARG))
				PRINTFORML 
				PRINTFORMW 「ふぅ、これでいいでしょう」
				PRINTFORMW 「これからは先ほどまでのようなケモノと変わらないような仕草は慎むように」
				PRINTFORML 
				PRINTFORMW %CALLNAME:ARG%の躾けによって%CALLNAME:発情要求キャラ番号%の火照りは収まった
				PRINTFORML 
				CFLAG:発情要求キャラ番号:던전内発情用욕정치 = 0
				IF GET_STATE(CFLAG:発情要求キャラ番号:ステート) == "CHARM" || GET_STATE(CFLAG:発情要求キャラ番号:ステート) == "HAPPY"
					CFLAG:発情要求キャラ番号:ステート = 0
					CFLAG:発情要求キャラ番号:ステート経過ターン = 0
				ENDIF

				;逃げる
				RETURN 5

			;ただヤラれる
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 
						PRINTFORMW 「ほら、何を惚けているのですか？早くしなさい、グズめ」
						PRINTFORMW 「使ってやったのですから礼と共に掃除をするのは当然でしょう？」
						PRINTFORML 
						PRINTFORMW 「躾けてやっているのに何を悦んでいるんですか、このメス豚は」
						PRINTFORML 
					CASE 1
						PRINTFORML 
						PRINTFORML 「そう、そうです」
						PRINTFORMW 「もっと腰を突き上げ、%CSTR:ARG:11%の方に押し付けなさい」
						PRINTFORML 
						PRINTFORML 「いいですよ、そうして締め付けるのです」
						PRINTFORMW 「使っていただいてる、という感謝の気持ちを忘れてはいけませんよ？フフフッ」
						PRINTFORML 
				ENDSELECT
				PRINTFORML 
				PRINTFORMW %CALLNAME:発情要求キャラ番号%は%CALLNAME:ARG%に躾と称されながら好き勝손犯されている……
				PRINTFORML 
				TCVAR:発情要求キャラ番号:획득조교경험 = 7
				CALL SEXIAL_NEGOCIATE, ARG, 発情要求キャラ番号
				
				PRINTFORML 
				PRINTFORML 「良い躾が出来ました」
				PRINTFORMW 「汗もかきましたので、この辺で失礼いたしますよ」
				;逃げる
				RETURN 5
				
			ENDIF
		
		;断る
		CASE 2
			;関数『仮_異性を要求するか？』を使って怒るかどうかを決める
			;通常は２０％で怒る
			IF RAND:100 < 20*(1+仮_異性を要求するか？(ARG))
				PRINTFORML 
				PRINTFORML 「%CSTR:ARG:11%の行為を無碍に扱うとは…」
				PRINTFORMW 「許しません！」
				PRINTFORML 
				;奇襲
				RETURN 3

			;怒らない
			ELSE
				PRINTFORML 
				PRINTFORML 「そうですか、それは잔ネンですね」
				PRINTFORMW 「%CSTR:ARG:11%の躾は評判が良かったのですが」
				PRINTFORML 
				;회화継続
				RETURN 0
			
			ENDIF
		
		
	ENDSELECT

ENDIF
$SKIP_発情キャラ要求

;質問を増やした場合、RAND:XのXを増やす
;RETURNの値により回答後の雰囲気が変わる。
;0.회화継続 1.直接仲魔になる 2.何かくれる 3.奇襲される 4.적対する 5.逃げる 6.気まずくなる
SELECTCASE RAND:1
	CASE 0
		IF IS_EROEQUIP_F(PLAYER)
			IF TALENT:ARG:천사
				PRINTFORML 「人の子よ、何故その様に肌を노출しているのですか」
				PRINTFORMW 「慎みを忘れてはなりません」
			ELSEIF TALENT:ARG:잔학
				PRINTFORML 「瑞々しい肌ですね……」
				PRINTFORMW 「無茶苦茶にしてバラバラにしたくなります」
			ELSE
				PRINTFORML 「美味しそうな肌ですね……」
				PRINTFORMW 「食べごたえがありそうです」
			ENDIF
			IF TALENT:ARG:천사
				PRINTFORML
				PRINTFORML [1]強い장비だから　　[2]ごめんなさい
				PRINTFORML [3]ファッション　　　[4]유혹してみる
			ELSE
				PRINTFORML
				PRINTFORML [1]食べてみる？　　　　[2]怖がる
				PRINTFORML [3]愛想笑いを浮かべる　[4]身構える
			ENDIF
			$INPUT_LOOP_A0
			INPUT
			PRINTFORML
			IF RESULT == 1
				;１．食べて/強い장비
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
	;			素質によって판정値が랜덤に上下することもある
				SIF TALENT:ARG:잔학
					LOCAL += RAND:15
				IF LOCAL < 50
					CFLAG:ARG:友好度 -= RAND:20
					;실패
					IF TALENT:ARG:천사
						PRINTFORML 「自慢の장비を試してさしあげましょう」
						PRINTFORMW 「行きますよ！！」
						RETURN 3
					ELSEIF TALENT:ARG:잔학
						PRINTFORML 「さて…」
						PRINTFORMW 「さっそくいただきましょうか！！」
						RETURN 3
					ELSE
						PRINTFORML 「生憎ですが유혹には乗りません」
						PRINTFORMW 「油断するほど馬鹿ではないのですよ」
						RETURN 6
					ENDIF
				ELSE
					CFLAG:ARG:友好度 += RAND:50
					;성공
					IF TALENT:ARG:천사
						PRINTFORML 「장비ならばしょうがありませんね」
						PRINTFORMW 「ですが、慎みを忘れてはなりませんよ」
						RETURN 0
					ELSEIF TALENT:ARG:잔학
						PRINTFORML 「フフ……冗談ですよ」
						PRINTFORMW 「ですが、気の強い人は好きです」
						RETURN 0
					ELSE
						PRINTFORML 「フフ…」
						PRINTFORML 「악마などまるで恐ろしくないと言う눈ですね」
						PRINTFORMW 「気に入りましたよ」
						RETURN 0
					ENDIF
				ENDIF
			ELSEIF RESULT == 2
				;２．怖がる/ごめんなさい
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				SIF TALENT:ARG:잔학
					LOCAL += RAND:30
				SIF TALENT:ARG:천사
					LOCAL += RAND:30
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:50
					IF TALENT:ARG:천사
						PRINTFORML 「今回は許してさしあげましょう」
						PRINTFORMW 「ですが、謝る気持ちがあるのならば慎みある장비をなさい」
						RETURN 5
					ELSE
						PRINTFORML 「怯えろ！！　すくめ！！」
						PRINTFORMW 「仲魔の性能を活かせぬまま 死んでゆくがいい！！」
						RETURN 4
					ENDIF
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					;성공
					IF TALENT:ARG:천사
						IF RAND:2 == 0
							PRINTFORML 「今後はもっと慎みある장비をするようにしなさい」
							PRINTFORMW 「解りましたか」
							RETURN 0
						ELSE
							PRINTFORML 「過ちを認める事は大事ですよ」
							PRINTFORMW 「부끄럼쟁이を持つようにしなさい」
							RETURN 0
						ENDIF
					ELSE
						IF RAND:2 == 0
							PRINTFORML 「いい表情ですね」
							PRINTFORMW 「もっと怯えてください」
							RETURN 0
						ELSE
							PRINTFORML 「怖いですか？」
							PRINTFORMW 「ええ、%CSTR:ARG:11%は악마ですからね」
							RETURN 0
						ENDIF
					ENDIF
				ENDIF

			ELSEIF RESULT == 3
				;３．愛想笑い/ファッション
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				SIF TALENT:ARG:천사
					LOCAL -= RAND:30
				IF LOCAL < 100
					CFLAG:ARG:友好度 -= RAND:50
					;실패
					IF TALENT:ARG:천사
						PRINTFORML 「불쾌です」
						PRINTFORMW 「視界から消えていただきましょうか」
						RETURN 4
					ELSE
						PRINTFORML 「気持ち悪いですね」
						PRINTFORMW 「死んでください」
						RETURN 4
					ENDIF
				ELSE
					CFLAG:ARG:友好度 += RAND:20
					;성공
					IF TALENT:ARG:천사
						PRINTFORML 「若さゆえの過ちですか」
						PRINTFORMW 「理解は出来ますが許す事は出来ません」
						RETURN 6
					ELSE
						PRINTFORML 「…………」
						PRINTFORML 「得体の知れない인간ですね」
						PRINTFORMW 「係わり合いにならない方が良さそうです」
						RETURN 5
					ENDIF
				ENDIF

			ELSEIF RESULT == 4
				;４．身構える/유혹する
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2) - RAND:20
				SIF TALENT:ARG:천사
					LOCAL -= 30
				IF LOCAL < 80
					CFLAG:ARG:友好度 -= RAND:50
					;실패
					IF TALENT:ARG:천사
						PRINTFORML 「악마に魅入られし人の子よ」
						PRINTFORMW 「%CSTR:ARG:11%が撃ち滅ぼしてさしあげましょう」
						RETURN 3
					ELSE
						PRINTFORML 「いいでしょう」
						PRINTFORMW 「血で血を洗うパーティーの幕開けといきましょうか！！」
						RETURN 4
					ENDIF
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					;성공
					IF TALENT:ARG:천사
						PRINTFORML 「な、何を言っているのだ！！」
						PRINTFORMW 「神よ、お許しください！！」
						RETURN 5
					ELSE
						PRINTFORML 「おやおや」
						PRINTFORML 「身体が硬くなっていますよ」
						PRINTFORMW 「それほど무서움なくてもとって食ったりはしません」
						RETURN 0
					ENDIF
				ENDIF
			ELSE
				GOTO INPUT_LOOP_A0
			ENDIF
		;それ以外
		ELSE
;0.회화継続 1.直接仲魔になる 2.何かくれる 3.奇襲される 4.적対する 5.逃げる 6.気まずくなる
			IF TALENT:ARG:천사
				PRINTFORML 「인간の中では조교師という人や악마を売買する愚かな者がいると聞きました」
				PRINTFORMW 「人の子よ、そういう인간をどう思いますか？」
				PRINTFORML
				PRINTFORML [1]恥ずかしいと思う　[2]誇りに思う
				IF TALENT:PLAYER:140 == 1 || TALENT:PLAYER:157 == 0
					PRINTFORML [3]どうでもいい
				ELSE
					PRINTFORML [3]泣き出す　　　　　
				ENDIF
			ELSE
				PRINTFORMW 「인간の間では조교が流行っていると聞きましたが？」
				PRINTFORML
				PRINTFORML [1]流行っている　　　[2]知らない
				PRINTFORML [3]조교してください　
			ENDIF
			$INPUT_LOOP_B0
			INPUT
			PRINTFORML
			IF RESULT == 1
				;１．流行っている/恥ずかしい
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				SIF TALENT:ARG:겁쟁이
					LOCAL -= RAND:15
				SIF TALENT:ARG:얌전함
					LOCAL -= RAND:15
				SIF TALENT:ARG:잔학
					LOCAL += RAND:15
				IF TALENT:ARG:반항적
					IF TALENT:ARG:천사
						LOCAL += RAND:50
					ELSE
						LOCAL -= RAND:30
					ENDIF
				ENDIF
				SIF TALENT:ARG:천사
						LOCAL += RAND:50
				IF LOCAL < 50
					CFLAG:ARG:友好度 -= RAND:50
					;실패
					IF TALENT:ARG:천사
						CFLAG:ARG:友好度 += RAND:50
						PRINTFORML 「恥じる気持ちがあるのならば…｣
						PRINTFORML 「まずは自分の行いから見直しなさい」
						PRINTFORMW 「神は何時でも見ているのですよ」
						RETURN 5
					ELSEIF TALENT:ARG:잔학
						PRINTFORML 「奇遇ですね」
						PRINTFORMW 「%CSTR:ARG:11%も인간の조교が趣味なのです」
						RETURN 3
					ELSE
						PRINTFORML 「知っていますよ、流行っていない事ぐらい」
						PRINTFORMW 「%CSTR:ARG:11%を騙そうという腹つもりが気に入りません」
						RETURN 6
					ENDIF
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					;성공
					IF TALENT:ARG:천사
						CFLAG:ARG:友好度 += RAND:70
						PRINTFORML 「人の子よ、貴方の罪を許しましょう」
						PRINTFORMW 「神は等しく인간を愛しているのです」
						RETURN 0
					ELSEIF TALENT:ARG:잔학
						CFLAG:ARG:友好度 += RAND:10
						PRINTFORML 「最近の인간は中々趣味がいいですね」
						PRINTFORMW 「조교はとてもいいものです」
						RETURN 0
					ELSE
						PRINTFORML 「ほぅ…」
						PRINTFORMW 「物知りですね、勉強になりました」
						RETURN 0
					ENDIF
				ENDIF
			ELSEIF RESULT == 2
				;２．知らない/誇り
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				SIF TALENT:ARG:겁쟁이
					LOCAL += RAND:15
				SIF TALENT:ARG:얌전함
					LOCAL += RAND:15
				SIF TALENT:ARG:우애
					LOCAL += RAND:30
				SIF TALENT:ARG:영맹
					LOCAL -= RAND:10
				SIF TALENT:ARG:교활
					LOCAL -= RAND:20
				SIF TALENT:ARG:잔학
					LOCAL -= RAND:30
				SIF TALENT:ARG:천사
					LOCAL -= RAND:100
				IF LOCAL < 80
					CFLAG:ARG:友好度 -= RAND:50
					;실패
					IF TALENT:ARG:천사
						PRINTFORML 「愚かな…」
						PRINTFORMW 「この場で%CSTR:ARG:11%が貴方の誇りを砕いてさしあげましょう」
						RETURN 3
					ELSEIF TALENT:ARG:잔학 || TALENT:ARG:영맹
						PRINTFORML 「知らないなら教えてあげましょう」
						PRINTFORMW 「%CSTR:ARG:12%自身の身体でね」
						RETURN 3
					ELSE
						PRINTFORML 「本当に知らないのですか？」
						PRINTFORML 「貴方のその눈は嘘をついている」
						PRINTFORMW 「악마ですからそのぐらいは解りますよ」
						RETURN 6
					ENDIF
				ELSE
					IF TALENT:ARG:천사
						PRINTFORML 「悔い改めなさい」
						PRINTFORMW 「貴方には少し考える時間が필요のようです」
						RETURN 6
					ELSE
						CFLAG:ARG:友好度 += RAND:10
						PRINTFORMW 「知らないのですか…」
						PRINTFORMW 「それならばその方が幸せかもしれませんね」
						RETURN 0
					ENDIF
				ENDIF
			ELSEIF RESULT == 3
				;３．조교してください/どうでもいい/泣き出す
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:지혜+MAXBASE:PLAYER:운/2)
				SIF TALENT:ARG:호기심
					LOCAL += RAND:20
				SIF TALENT:ARG:우둔
					LOCAL += RAND:10
				SIF TALENT:ARG:프라이드높음
					LOCAL += RAND:20
				SIF TALENT:ARG:반항적
					LOCAL -= RAND:30
				IF TALENT:ARG:천사
					IF TALENT:PLAYER:140 == 1 || TALENT:PLAYER:157 == 0
						LOCAL -= 100
					ELSE
						LOCAL += 30
					ENDIF
				ENDIF
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:50
					;실패
					IF TALENT:ARG:천사
						PRINTFORML 「罪から눈を反らし生きていくというのですか」
						PRINTFORMW 「눈を깨달음ましなさい」
					ELSE
						PRINTFORML 「面白い申し出ですが」
						PRINTFORMW 「何処まで本気ですか？」
					ENDIF
					RETURN 6
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					IF TALENT:ARG:천사
						IF TALENT:PLAYER:140 == 1 || TALENT:PLAYER:157 == 0
							PRINTFORML 「무관심は罪ですよ」
							PRINTFORMW 「人の子よ、考え直しなさい」
							RETURN 6
						ELSE
							CFLAG:ARG:友好度 += RAND:50
							PRINTFORML 「泣き止むのです、人の子よ」
							PRINTFORMW 「神は貴方を責めているのではありませんよ」
							RETURN 0
						ENDIF
					ELSE
						IF TALENT:PLAYER:140 == 1 || TALENT:PLAYER:157 == 0
							PRINTFORML 「%CSTR:ARG:11%の下僕になりたいのですか？」
							PRINTFORMW 「ふむ…考えてみましょう」
							RETURN 0
						ELSE
							CFLAG:ARG:友好度 += RAND:50
							PRINTFORML 「ほぅ…」
							PRINTFORML 「%CSTR:ARG:11%の下僕になるのを望むのですか」
							PRINTFORMW 「悪い話ではありませんね」
							RETURN 0
						ENDIF
					ENDIF
				ENDIF
			ELSE
				GOTO INPUT_LOOP_B0
			ENDIF
		ENDIF
ENDSELECT

;===========================================================================
;회화時問答
;===========================================================================
@QUESTION_PUB318,ARG,ARG:1

PRINTFORML
CUSTOMDRAWLINE ･
PRINTFORML

DO

	;設問を増やした場合、RAND:XのXを増やす
	RESULT:1 = RAND:2

	;疲れてきたから손抜き

	;RETURNの値により回答後の雰囲気が変わる。
	;0.회화継続 1.直接仲魔になる 2.何かくれる 3.奇襲される 4.적対する 5.逃げる 6.気まずくなる 7.交渉開始
	SELECTCASE RESULT:1
		CASE ARG:1
			CONTINUE
		CASE 0
			PRINTFORMW 「%CSTR:ARG:12%は、仲魔をどう思っていますか？」
			PRINTFORML
			PRINTFORML [1]頼もしい　　[2]발손まとい
			PRINTFORML [3]合体素材
			$INPUT_LOOP_A2
			INPUT
