				RETURN 1
			;どんどんイキなさい！セリフ
			CASE 2
				PRINTFORMW 『それそれっ！%CSTR:ASSI:一人称%達の腰ふりでどんどんイッてね！』
				RETURN 1
			;とても気持ちよさそうセリフ
			CASE 3
				PRINTFORMW 『%CALLNAME%とても気持ちよさそうにしてるね。』
				RETURN 1
			;ここに入れてセリフ
			CASE 4
				PRINTFORMW 『さあ%CSTR:ASSI:二人称%、ここに入れてねー』
				RETURN 1
		ENDSELECT
