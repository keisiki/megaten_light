				RETURN 1
			;煽りに対してのセリフ
			CASE 3
				PRINTFORMW 『むっー！ここまで言われたら絶対に負けられないんだから！』
				RETURN 1
			;いつもより感じてるねというセリフ
			CASE 4
				PRINTFORMW 『%CSTR:ASSI:二人称%、とっても気持ちよさそうにだね』
				RETURN 1
			;お互いに気持ちいいねセリフ
			;CASE 5
			;	PRINTFORMW 『』
			;	RETURN 1
			;気持ちいいセリフ
			CASE 6
