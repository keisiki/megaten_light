	CASE 185
		SELECTCASE ARG
			;犯されてとても気持ちいいセリフ
			CASE 1
				PRINTFORMW 『あん、あん、気持ちいいよぉ%UNICODE(0x2661)%』
				RETURN 1
			;私たち、とっても仲がいいでしょ？セリフ
			CASE 2
				PRINTFORMW 『%CSTR:ASSI:一人称%と%CSTR:ASSI:二人称%はとっても仲良しなの%UNICODE(0x2661)%』
				PRINTFORMW 『ね、%CSTR:ASSI:二人称%%UNICODE(0x2661)%』
				RETURN 1
			;うらやましいでしょ？セリフ
			CASE 3
				PRINTFORMW 『えへへ～、とても羨ましいでしょ～』
				RETURN 1
			;こっちが気になってるみたいだねセリフ
			CASE 4
				PRINTFORMW 『ねえねえ、%CALLNAME%こっちが気になってるみたいだね』
				RETURN 1
		ENDSELECT

;-------------------------------------------------
;193,콘돔정음（조수）
;-------------------------------------------------
	CASE 193
		SELECTCASE ARG
			;정액とてもおいしいセリフ
			CASE 1
				PRINTFORMW 『んじゅ、ぢゅるる…はぁ%UNICODE(0x2661)%정액美味しい♪』
				RETURN 1
			;これはあたしのものよ！セリフ
			CASE 2
				PRINTFORMW 『これは%CSTR:ASSI:一人称%の物なんだからね！』
				PRINTFORMW 『%CALLNAME%なんかにあげないんだから！んじゅう…ちゅる』
				RETURN 1
			;うらやましいでしょ？セリフ
			CASE 3
				PRINTFORMW 『羨ましいでしょ？』
				RETURN 1
			;先にいただくわセリフ（仲がいい）
			CASE 4
				PRINTFORMW 『%CSTR:ASSI:一人称%が先に정액貰うよ』
				RETURN 1
		ENDSELECT
