IF P > PALAMLV:3 && CFLAG:251 == 0
	CALL PALAM_MESSAGE_D
	;미약を使った場合
	IF SELECTCOM == 181
		PRINTFORMW 「…そんな薬で人の心を如何こうしようだなんて、%CSTR:二人称%はつくづくろくでなしだね」
	;それ以外
	ELSE
		PRINTFORMW 「(参ったな、身体が大分に火照ってきた…)」
	ENDIF
	CFLAG:251 = 1
ENDIF

