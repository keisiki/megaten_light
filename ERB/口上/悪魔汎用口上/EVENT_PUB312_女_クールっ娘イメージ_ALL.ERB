;-------------------------------------------------
;奴隷の절정時のテ키스ト
;-------------------------------------------------
;-------------------------------------------------
;初めてC절정
;-------------------------------------------------
IF NOWEX:0 > 0 && CFLAG:242 == 0
	PRINTFORMW 「あっ、な、こ、れは…あああ！」
	PRINTFORMW %CALLNAME%は肉芽からの初めての감각に戸惑いながら嬌声を上げた。
	CFLAG:242 = 1
ENDIF

;-------------------------------------------------
;初めてV절정
;-------------------------------------------------
IF NOWEX:1 > 0 && CFLAG:243 == 0
	PRINTFORMW 「あっ…いやだっ、これっ…んああんっ！」
	PRINTFORMW 度重なる%CALLNAME:MASTER%の攻めにより、とうとう%CALLNAME%は女の悦びを味わうこととなった…
	CFLAG:243 = 1
ENDIF

;-------------------------------------------------
;初めてA절정
;-------------------------------------------------
IF NOWEX:2 > 0 && CFLAG:244 == 0
	PRINTFORMW 「そんな…こ、これ…いや、いやあああ！」
	PRINTFORMW 遂に%CALLNAME%は애널で達し、その顔を羞恥で紅潮させつつ放心している。
	CFLAG:244 = 1
ENDIF

;-------------------------------------------------
;初めてB절정
;-------------------------------------------------
IF NOWEX:3 > 0 && CFLAG:245 == 0
	PRINTFORMW 「あ、む、가슴が、おかしく…なあ、ああん！」
	PRINTFORM %CALLNAME%は
	IF (TALENT:111 || TALENT:112)
		PRINTFORMW その豊満な가슴への丹念な攻めに耐え切れず、ぶるっと体を震わせて절정した。
		PRINTFORMW 荒い息を吐きながら痙攣している身体に合わせて柔らかく揺れている가슴が実に扇情的だ。
	ELSEIF (TALENT:109 || TALENT:110)
		PRINTFORMW その小さな가슴への攻めにとうとう根を上げ、赤面しながら崩れ落ちた。
	ELSE
		PRINTFORMW %CALLNAME:MASTER%による가슴への攻めに耐え兼ね、赤面しながら痙攣した。
	ENDIF
	CFLAG:245 = 1
ENDIF

;-------------------------------------------------
;절정
;-------------------------------------------------
;強절정
IF TFLAG:29 > 1
	CALL PALAM_MESSAGE_B
	;함락済み
	IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:246 < 4 || FLAG:7 == 2)
		;연모、친애
		IF TALENT:3 || TALENT:6
			IF RAND:2 == 0
				PRINTFORMW 「お願いっぎゅって抱いて、抱いて！何処かに行っちゃいそうなんだっ！」
				PRINTFORMW そういわれた%CALLNAME:MASTER%は%CALLNAME%をしっかりと抱きしめてやった。
				PRINTFORMW 「あっ、あっ、あああああんっ！」
				PRINTFORM %CALLNAME%は絶叫し、
				IF TALENT:111 || TALENT:112
					PRINTFORM その豊満な가슴を揺らしながら
				ENDIF
				PRINTFORMW 痙攣し続けた…
			ELSE
				PRINTFORMW 「あ、来る、来る、きっああああんっ！」
				PRINTFORMW %CALLNAME%は절정して少しの間失神していたようだ…
			ENDIF
		;음란、창부
		ELSEIF TALENT:4 || TALENT:7
			PRINTFORMW 
		;복종、예속
		ELSEIF TALENT:5 || TALENT:8
			PRINTFORMW 
		ENDIF
		CFLAG:246 = 4
	;굴복각인Lv3
	ELSEIF MARK:2 == 3 && (CFLAG:246 < 3 || FLAG:7 == 2)
		PRINTFORMW 「あ、も、もう、もうだめだっあああああ！」
		PRINTFORMW 只管に歯を食いしばって저항していた%CALLNAME%もとうとう折れたか、或いは開発した体の影響か。
		PRINTFORMW %CALLNAME%は恥も外聞もない絶叫を上げて痙攣し続けている。
		CFLAG:246 = 3
	;それ以外
	ELSEIF CFLAG:246 < 2 || FLAG:7 == 2
		PRINTFORMW 「あ、やめ、おかしくなるぅうう！」
		PRINTFORMW 開発された体に引っ張られたか、激しい絶叫を上げながら%CALLNAME%は痙攣し、しばらく動かなかった…
		CFLAG:246 = 2
	ENDIF
;절정
ELSEIF TFLAG:29 > 0
	CALL PALAM_MESSAGE_B
	;함락済み
	IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:246 < 4 || FLAG:7 == 2)
		;연모、친애
		IF TALENT:3 || TALENT:6
			PRINTFORMW 「あ、んあ、イっちゃうぅうう！」
			PRINTFORMW 悲鳴とも取れる嬌声を上げて절정した%CALLNAME%は%CALLNAME:MASTER%に非難するような눈を向けてきた。
			PRINTFORMW 「…まったく、%CSTR:二人称%は相変わらず激しいね」
		;음란、창부
		ELSEIF TALENT:4 || TALENT:7
			PRINTFORMW 
			PRINTFORMW 
		;복종、예속
		ELSEIF TALENT:5 || TALENT:8
			PRINTFORMW 
		ENDIF
		CFLAG:246 = 4
	;굴복각인Lv3
	ELSEIF MARK:2 == 3 && (CFLAG:246 < 3 || FLAG:7 == 2)
		PRINTFORMW 「あ、も、もう、もうだめだっあああああ！」
		PRINTFORMW 只管に歯を食いしばって저항していた%CALLNAME%もとうとう折れたか、或いは開発した体の影響か。
		PRINTFORMW %CALLNAME%は悲鳴と共に절정した。
		CFLAG:246 = 3
	;それ以外
	ELSEIF CFLAG:246 < 2 || FLAG:7 == 2
		PRINTFORMW 「あ、やめ、おかしくなるぅうう！」
		PRINTFORMW 開発された体に引っ張られたか、暫くの間激しい絶叫を上げながら%CALLNAME%は痙攣していた。
		CFLAG:246 = 2
	ENDIF
ENDIF

;-------------------------------------------------
;사정
;-------------------------------------------------
;사정している
IF TFLAG:10 > 0
	;함락済み
	IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:247 < 3 || FLAG:7 == 2)
		PRINTFORMW 「んはぁ…で、出ちゃうっ、%CSTR:一人称%、女なのにっはあんっ%UNICODE(0x2661)%」
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			PRINTFORMW 「…ふふ、今度は%CSTR:一人称%が%CSTR:二人称%のを搾り取ってあげるよ…」
		ENDIF
		CFLAG:247 = 3
	;굴복각인Lv3
	ELSEIF MARK:2 == 3 && (CFLAG:247 < 2 || FLAG:7 == 2)
		IF CFLAG:801 == 1
			PRINTFORMW 「んんっ…つくづくこんなものが自分についていることが煩わしいよ」
		ELSE
			PRINTFORMW 「んはぁ…この、鬼畜」
		ENDIF 
		CFLAG:247 = 2
	;それ以外
	ELSEIF CFLAG:247 < 1 || FLAG:7 == 2
		IF CFLAG:801 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		CFLAG:247 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;분유
;-------------------------------------------------
;분유している
IF TFLAG:11 > 0
	;함락済み
	IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:248 < 3 || FLAG:7 == 2)
		PRINTFORMW 「んっ…参ったね、%CSTR:一人称%のミルクが溢れてしまって、暫く止まってくれそうにない」
		PRINTFORMW 「折角だし、%CSTR:二人称%に少し飲んでもらおうかな」
		PRINTFORM %CALLNAME%のお言葉に甘えて、
		IF (TALENT:111 || TALENT:112)
			PRINTFORM その豊満な가슴から
		ELSEIF (TALENT:109 || TALENT:110)
			PRINTFORM そのなだらかな平原から
		ENDIF
		PRINTFORMW 溢れる모유をいただくことにした…
		PRINTFORMW 「…そんなに、その、%CSTR:一人称%のミルクはおいしいのかい？」
		CFLAG:248 = 3
	;굴복각인Lv3
	ELSEIF MARK:2 == 3 && (CFLAG:248 < 2 || FLAG:7 == 2)
		PRINTFORMW 
		CFLAG:248 = 2
	;それ以外
	ELSEIF CFLAG:248 < 1 || FLAG:7 == 2
		PRINTFORMW 
		CFLAG:248 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;방뇨
;-------------------------------------------------
;방뇨している
IF TFLAG:29 > 0 && (TEQUIP:27 || TALENT:57)
	;함락済み
	IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:249 < 3 || FLAG:7 == 2)
		PRINTFORMW 
		CFLAG:249 = 3
	;굴복각인Lv3
	ELSEIF MARK:2 == 3 && (CFLAG:249 < 2 || FLAG:7 == 2)
		PRINTFORMW 
		CFLAG:249 = 2
	;それ以外
	ELSEIF CFLAG:249 < 1 || FLAG:7 == 2
		PRINTFORMW 
		CFLAG:249 = 1
	ENDIF
ENDIF

