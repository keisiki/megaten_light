	CASE 124
			SELECTCASE ARG
				;一緒に気持ちよくしようというセリフ
				CASE 1
					PRINTFORMW %CALLNAME:ASSI%は%CALLNAME%と더블펠라をしようと誘っている
				;私のほうが気持ちよくしてるというセリフ
				CASE 2
					PRINTFORMW %CALLNAME:ASSI%は%CALLNAME%より自分のフェラのほうがうまいといった
				;煽りに対してのセリフ
				CASE 3
					PRINTFORMW %CALLNAME:ASSI%は%CALLNAME%により煽りを受けて少し気が立っている
				;いつもより感じてるねというセリフ
				CASE 4
					PRINTFORMW %CALLNAME:ASSI%は더블펠라により%CALLNAME:MASTER%がいつもより感じているといった
				;레즈っぽいセリフに対するセリフ
				CASE 5
					IF ABL:ASSI:레즈끼 >= 3 && !TALENT:남자
						PRINTFORMW %CALLNAME:ASSI%は%CALLNAME%とフェラをすることがうれしいようだ
					ELSE	
						PRINTFORMW %CALLNAME:ASSI%は%CALLNAME%の言うことが理解できなかった
					ENDIF
				;二人のフェラは気持ちいいでしょ？セリフ
				CASE 6
					PRINTFORMW %CALLNAME:ASSI%は더블펠라の感想を%CALLNAME:MASTER%に問いかけている
			ENDSELECT

