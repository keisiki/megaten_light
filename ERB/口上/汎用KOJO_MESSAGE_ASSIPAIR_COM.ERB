;-------------------------------------------------
;122,조수를범하게한다
;-------------------------------------------------
	CASE 122
		SELECTCASE ARG
			;気持ちよくて感じている
			CASE 1
				PRINTFORMW %CALLNAME:ASSI%は気持ちよさそうだ
			;こっちに来てと言うセリフ
			CASE 2
				PRINTFORMW %CALLNAME:ASSI%は%CALLNAME%を誘っている
			;大丈남편というセリフ
			CASE 3
				PRINTFORMW %CALLNAME:ASSI%は%CALLNAME%に犯されても平気のようだ
			;犯されて強がりを言うセリフ
			CASE 4
				PRINTFORMW %CALLNAME%に犯されて%CALLNAME:ASSI%は強がりを言っている
			;うまくなってきたねセリフ
			CASE 5
				PRINTFORMW %CALLNAME:ASSI%は%CALLNAME%がうまくなっているのを感じているようだ
		ENDSELECT
		
;-------------------------------------------------
;124,더블펠라
