;========================================
;불결이동 함수
;ARG의 ARGS:1과 ARG:1의 ARGS 사이의 불결을 서로 이동하다
;========================================
@MOVE_STAIN(ARGS, ARG, ARGS:1, ARG:1)
#LOCALSIZE 1
SELECTCASE ARGS:1
	CASE "입"
		LOCAL = 0
	CASE "손"
		LOCAL = 1
	CASE "페니스"
		LOCAL = 2
	CASE "바기나"
		LOCAL = 3
	CASE "애널"
		LOCAL = 4
	CASE "가슴"
		LOCAL = 5
	CASE "질내"
		LOCAL = 6
	CASE "머리카락"
		LOCAL = 7
	CASE "모자"
		LOCAL = 10
	CASE "옷"
		LOCAL = 11
	CASE "하의"
		LOCAL = 12
	CASE "전신복"
		LOCAL = 13
	CASE "장갑"
		LOCAL = 14
	CASE "속옷（상）"
		LOCAL = 15
	CASE "속옷（하）"
		LOCAL = 16
	CASE "전신속옷"
		LOCAL = 17
	CASE "양말"
		LOCAL = 18
	CASE "신발"
		LOCAL = 19
	CASE "겉옷"
		LOCAL = 20
ENDSELECT
SELECTCASE ARGS
	CASE "입"
		STAIN:ARG:0 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:0
	CASE "손"
		STAIN:ARG:1 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:1
	CASE "페니스"
		STAIN:ARG:2 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:2
	CASE "바기나"
		STAIN:ARG:3 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:3
	CASE "애널"
		STAIN:ARG:4 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:4
	CASE "가슴"
		STAIN:ARG:5 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:5
	CASE "질내"
		STAIN:ARG:6 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:6
	CASE "머리카락"
		STAIN:ARG:7 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:7
	CASE "모자"
		STAIN:ARG:10 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:10
	CASE "옷"
		STAIN:ARG:11 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:11
	CASE "하의"
		STAIN:ARG:12 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:12
	CASE "전신복"
		STAIN:ARG:13 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:13
	CASE "장갑"
		STAIN:ARG:14 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:14
	CASE "속옷（상）"
		STAIN:ARG:15 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:15
	CASE "속옷（하）"
		STAIN:ARG:16 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:16
	CASE "전신속옷"
		STAIN:ARG:17 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:17
	CASE "양말"
		STAIN:ARG:18 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:18
	CASE "신발"
		STAIN:ARG:19 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:19
	CASE "겉옷"
		STAIN:ARG:20 |= STAIN:(ARG:1):LOCAL
		STAIN:(ARG:1):LOCAL = STAIN:ARG:20
ENDSELECT