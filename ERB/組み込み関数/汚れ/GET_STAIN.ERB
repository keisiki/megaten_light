;========================================
;불결판정関数
;ARGのARGSにARGS:1の불결がついているかを판정する
;========================================
@GET_STAIN(ARGS, ARGS:1, ARG = -99)
#FUNCTION
#LOCALSIZE 1
SIF ARG == -99
	ARG = TARGET
SELECTCASE ARGS
	CASE "입"
		LOCAL = 0
	CASE "손"
		LOCAL = 1
	CASE "페니스"
		LOCAL = 2
	CASE "바기나"
		LOCAL = 3
	CASE "애널"
		LOCAL = 4
	CASE "가슴"
		LOCAL = 5
	CASE "질내"
		LOCAL = 6
	CASE "머리카락"
		LOCAL = 7
	CASE "모자"
		LOCAL = 10
	CASE "옷"
		LOCAL = 11
	CASE "하의"
		LOCAL = 12
	CASE "전신복"
		LOCAL = 13
	CASE "장갑"
		LOCAL = 14
	CASE "속옷（상）"
		LOCAL = 15
	CASE "속옷（하）"
		LOCAL = 16
	CASE "전신속옷"
		LOCAL = 17
	CASE "양말"
		LOCAL = 18
	CASE "신발"
		LOCAL = 19
	CASE "겉옷"
		LOCAL = 20
ENDSELECT
SELECTCASE ARGS:1
	CASE "애액"
		RETURNF GETBIT(STAIN:ARG:LOCAL, 0)
	CASE "페니스"
		RETURNF GETBIT(STAIN:ARG:LOCAL, 1)
	CASE "정액"
		RETURNF GETBIT(STAIN:ARG:LOCAL, 2)
	CASE "애널"
		RETURNF GETBIT(STAIN:ARG:LOCAL, 3)
	CASE "모유"
		RETURNF GETBIT(STAIN:ARG:LOCAL, 4)
	CASE "점액"
		RETURNF GETBIT(STAIN:ARG:LOCAL, 5)
	CASE "파과혈"
		RETURNF GETBIT(STAIN:ARG:LOCAL, 6)
ENDSELECT
