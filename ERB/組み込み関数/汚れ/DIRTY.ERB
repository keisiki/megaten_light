;-------------------------------------------------
;불결판정関数
;ARGのARGSについている불결を판정する
;애액と모유と元々その部位についていた불결は대상外
;불결の個数と、불결の합계(つまり、애액と모유を除いた불결情報を返す)
;-------------------------------------------------
@DIRTY, ARGS, ARG = -99
#FUNCTION
RESULT = 0
RESULT:1 = 0
SIF ARG == -99
	ARG = TARGET
;IF ARGS != 바기나" && GET_STAIN(ARGS, "애액", ARG)
;	RESULT++
;	RESULT:1 |= 1
;ENDIF
IF ARGS != "페니스" && GET_STAIN(ARGS, "페니스", ARG)
	RESULT++
	RESULT:1 |= 2
ENDIF
IF GET_STAIN(ARGS, "정액", ARG)
	RESULT++
	RESULT:1 |= 4
ENDIF
IF ARGS != "애널" && GET_STAIN(ARGS, "애널", ARG)
	RESULT++
	RESULT:1 |= 8
ENDIF
;IF GET_STAIN(ARGS, "모유", ARG)
;	RESULT++ 
;	RESULT:1 |= 16
;ENDIF
IF GET_STAIN(ARGS, "점액", ARG)
	RESULT++
	RESULT:1 |= 32
ENDIF
IF GET_STAIN(ARGS, "파과혈", ARG)
	RESULT++
	RESULT:1 |= 64
ENDIF
RETURNF RESULT 
