;-------------------------------------------------
;PBAND사용가능판정
;ARG = キャラ등록번호
;-------------------------------------------------
@USE_PBAND,ARG
#FUNCTION
;페니스밴드を持っていないとダメ
SIF !ITEM:페니스밴드
	RETURNF 0

	;--------------------------------
	;사정関連の処理で
	;페니스と페니스밴드を区別できるようになるまでは
	;--------------------------------
	;페니스があるとダメ
	SIF HAVE_PENIS(ARG)
		RETURNF 0
		
;클리캡사용중はダメ
SIF TEQUIP:ARG:11
	RETURNF 0

RETURNF 1
