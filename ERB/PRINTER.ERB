@PRINTER(TEXT)
#DIMS DYNAMIC TEXT
#DIM DYNAMIC STRSTART
#DIM DYNAMIC STREND
#DIM DYNAMIC STRLENGTH

STRLENGTH = STRLENSU(TEXT)

WHILE 1
STREND = STRFINDU(TEXT, "\t", STRSTART)
SIF STREND < 0
BREAK

PRINTSW SUBSTRINGU(TEXT, STRSTART, STREND - STRSTART)

STRSTART = STREND + 1
WEND

PRINTS SUBSTRINGU(TEXT, STRSTART, STRLENGTH - STRSTART)