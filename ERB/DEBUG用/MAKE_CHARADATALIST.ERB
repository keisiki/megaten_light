;攻略데이터作成のための데이터をはかせる関数
;LOCAL配列にキャラ데이터
;ARGに랭크をいれてGO
;LOCALSに中身をいれることで区切りをいれられます
@SHOPCOMABLE_195
[IF_DEBUG]
	RESULTS = 캐릭터데이터출력
	RETURN 1
[ENDIF]
IF FLAG:DEBUG
	RESULTS = 캐릭터데이터출력
	RETURN 1
ENDIF
RETURN -1


@SHOP_COM_195
#DIM LEVEL , 100
CLEARLINE 3000
VARSET LOCALS
VARSET LOCAL, -1
VARSET LEVEL
FOR LOCAL, 1 , 100
	SIF LOCAL:LOCAL == -1
		BREAK
	IF STRLENS(LOCALS:LOCAL)
		PRINTL 
		PRINTL =======================================================================
		PRINTSL LOCALS:LOCAL
		PRINTL =======================================================================
		PRINTL 출현악마
		PRINTL =======================================================================
	ENDIF
	CALL INCREASE_ANALYZE, LOCAL:LOCAL,1000
	CALL SET_ENEMY, 7 , LOCAL:LOCAL, LEVEL:LOCAL
	U = CHARANUM - 1
	;なにかしら강화をいれるときはここを使うといいでしょう
	SELECTCASE NO:U
	ENDSELECT
	PRINTFORML %CALLNAME:U% \@LEVEL:LOCAL?R{LEVEL:LOCAL}#\@
	CALL SHOW_INFO_PAGE_2
	CALL 캐릭터삭제, U
	PRINTL 
NEXT
WAIT


